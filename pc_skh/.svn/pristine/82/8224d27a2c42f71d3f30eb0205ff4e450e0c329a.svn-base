.swiper-container{position: relative; width: 100%; height: auto; min-width: 1200px;}
.swiper-wrapper{width: 100%; height: auto;}
.swiper-slide,.swiper-slide img{width: 100%; height: auto;}
.swiper-pagination.swiper-pagination-bullets{position: absolute; bottom: 132px; left: 0; right: 0; margin: auto; z-index: 999;}
.swiper-pagination8{z-index: 999;}

.w1140{width: 1140px; margin: 0 auto;}
h2{line-height: 60px; text-align: center; font-size: 26px; color: #333333;}
h2 em{padding: 14px 0; border-bottom: 3px solid #077478;}
p{font-size: 14px; color: #666666;}
.banner{padding-top: 102px; height: 420px; min-width: 1200px; background: url('/Public/images/service/fundusDiseases/banner.jpg') no-repeat center; background-size: cover; overflow: hidden; box-sizing: border-box;}
.banner p{color: #ffffff;}
.banner p.title{margin-left: 112px; width: 580px; height: 102px; font-size: 60px; border-bottom: 1px solid #ffffff;}
.banner p.desc{margin: 20px 0 0 112px; width: 536px; line-height: 42px; font-size: 24px;}
.con{width: 100%; padding-top: 72px; overflow: hidden; min-width: 1200px; box-sizing: border-box;}
.con1{height: 440px;}
.con1 dl{margin: 60px 0 0 74px; width: 330px; color: #333333; float: left;}
.con1 dl:nth-of-type(1){margin-left: 30px;}
.con1 dl:nth-of-type(2) dt{background-image: url('/Public/images/service/fundusDiseases/icon2.png');}
.con1 dl:nth-of-type(3) dt{background-image: url('/Public/images/service/fundusDiseases/icon3.png');}
.con1 dl dt{padding-left: 72px; line-height: 60px; font-size: 18px; background: url('/Public/images/service/fundusDiseases/icon1.png') no-repeat 0 center;}
.con1 dl em{padding-bottom: 18px; border-bottom: 2px solid #077478;}
.con1 dl dd{margin-top: 28px; font-size: 14px; color: #333333;}

.con2{height: 686px; background: #f5f5f5;}
.con2 p.title{margin-top: 50px; line-height: 30px; text-align: center;}
.con2 p.desc{margin-top: 80px; line-height: 28px; text-align: center;}
.con2 p.desc::before{content: ''; display: inline-block; margin: 10px 20px 0 0; width: 40px; height: 5px; border-top: 1px solid #bbbbbb;}
.con2 p.desc::after{content: ''; display: inline-block; margin: 10px 0 0 20px; width: 40px; height: 5px; border-top: 1px solid #bbbbbb;}
.con2 dl{margin: 32px 0 0 40px; float: left;}
.con2 dl:nth-of-type(1){margin-left: 0;}
.con2 dl dd{line-height: 74px; text-align: center; font-size: 14px; color: #077478;}

.con3{height: 660px;}
.con3 dl{height: 460px;}
.con3 dl dt{float: left; margin-right: 80px; width: 530px; height: 460px;background: url('/Public/images/service/fundusDiseases/pic5.jpg') no-repeat center; background-size: 100%;}
.con3 dl dt p{margin-left: 54px;}
.con3 dl dt p.title{margin-top: 314px; line-height: 44px; font-size: 30px;}
.con3 dl dt p.desc{line-height: 40px; font-size: 18px; color: #077478;}
.con3 dl dd{float: left; width: 530px;}
.con3 dl dd p.title{margin-top: 14px; line-height: 44px; font-size: 18px; color: #077478;}
.con3 dl dd p.desc{line-height: 20px;}

.con4{position: relative; height: 824px; background: #f5f5f5;}
.con4 dl{position: relative; width: 340px; height: 400px; margin: 70px 58px 0 0; padding-top: 36px; background: #ffffff; float: left; text-align: center; box-shadow: 0 0 10px #f1f0f0; box-sizing: border-box;}
.con4 dl:nth-of-type(1){margin-left: 30px;}
.con4 dl:nth-of-type(3){margin-right: 30px;}
.con4 dl img{width: 150px; height: auto;}
.con4 dl p.title{margin: 20px 0 10px 0; font-size: 20px; color: #333333; font-weight: bold;}
.con4 dl p.desc{width: 280px; margin: 0 auto; font-size: 14px; color: #666666;}
.con4 dl p.text{width: 280px; margin: 22px auto 0; line-height: 22px; font-size: 14px; color: #333333;}
.swiper-pagination-bullet-active{background: #077478;}
.swiper-pagination-bullet{width: 12px; height: 12px; margin-right: 14px;}
.swiper-pagination-bullet:nth-child(4){margin-right: 0;}
.swiper-button-prev{width: 40px; height: 70px; background: url('/Public/images/index/left1.png') no-repeat center;}
.swiper-button-prev:hover{background: url('/Public/images/index/left2.png') no-repeat center;}
.swiper-button-next{width: 40px; height: 70px; background: url('/Public/images/index/right1.png') no-repeat center;}
.swiper-button-next:hover{background: url('/Public/images/index/right2.png') no-repeat center;}
