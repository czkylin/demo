    <script>
      var sc0 = document.getElementsByClassName('swiper-container0');
      var myswiper = [mySwiper1, mySwiper2, mySwiper3, mySwiper4, mySwiper5, mySwiper6, mySwiper7];
      console.log(myswiper);
      function create(target, pic, name, rank, spe) {
        target.innerHTML += '<dl>'+'<dt>'+'<img src="'+pic+'" alt="" />'+'</dt>'+'<dd>'+'<p class="title">'+name+'</p>'+'<p class="desc">'+rank+'</p>'+'<p class="text">'+spe+'</p>'+'<a href="http://pbt.zoosnet.net/LR/Chatpre.aspx?id=PBT44701575&lng=cn&p=%e9%a2%84%e7%ba%a6%e4%b8%93%e5%ae%b6">预约专家</a>'+'</dd>'+'</dl>';
      }
      showAjax(0);
      function showAjax(oIndex) {  //oIndex为点击的li的索引
        var type_id = '11119';
        if(oIndex == 1) type_id = '11120';
        if(oIndex == 2) type_id = '11121';
        if(oIndex == 3) type_id = '11122';
        if(oIndex == 4) type_id = '11123';
        if(oIndex == 5) type_id = '11124';
        if(oIndex == 6) type_id = '11125';
        $.ajax({
          url: '/member/index/index_list',
          type: 'GET',
          dataType: 'json',
          data:{"type_id": type_id},
          success:function(data) {
            var list = data;
            if(sc0[oIndex].children[0].children.length == 0){
              list.forEach(function(item, index, arr) {
                var slide = document.createElement('div');
                slide.classList.add('swiper-slide', 'swiper-no-swiping');
                sc0[oIndex].children[0].appendChild(slide);
                item.doc_list.forEach(function(item, index, arr) {
                  create(slide, item.ACT_PIC, item.ACT_LECTOR, item.ACT_TITLE, item.ACT_ADDRESS);
                })
              })
              myswiper[oIndex].update(true);
            }
          }
        })
        .fail(function() {
          console.log("error");
        })
      }

      var toTop = document.getElementsByClassName('toTop')[0];
      toTop.onclick = function() {
        document.documentElement.scrollTop = document.body.scrollTop = 0;
      }
      $(window).scroll(function() {
        if(document.body.scrollTop > 464){
          toTop.style.display = 'block';
        } else {
          toTop.style.display = 'none';
        }
      })


    </script>
