/*一排样式*/
.con3{padding-top: 128px; height: 720px; background: #f5f5f5; overflow: inherit; box-sizing: border-box;}
.con3 .service{margin: 0 24px 30px 0; width: 282px; float: left;}
.con3 .service .top{position: relative; padding: 26px 0 0 30px; height: 118px; background: url('/Public/images/index/pic5.jpg') no-repeat center; background-size: 100%; box-sizing: border-box; color: #ffffff;}
.con3 .service .bottom{padding: 20px; height: 180px; background: #ffffff; box-sizing: border-box;}
.con3 .service .bottom li{position: relative; padding-left: 16px; height: 36px; line-height: 36px; border-bottom: 1px dashed #dddddd; font-size: 14px; color: #333333; overflow: hidden; text-overflow: ellipsis; white-space: nowrap;}
.con3 .service .top p.desc{font-size: 14px;}
.con3 .service .top p.title{font-size: 28px;}
.con3 .service .top a{transition: opacity .35s; position: absolute; left: 0; right: 0; bottom: 16px; margin: auto; display: block; width: 70px; height: 30px; line-height: 30px; border: 1px solid #ffffff; opacity: 0; font-size: 12px; color: #ffffff; text-align: center; border-radius: 10px;}
.con3 .service .top i{display: block; width: 72px; height: 4px; margin-top: -2px; background: #f7922e; opacity: .8;}
.con3 .service:nth-of-type(4){margin-right: 0;}

.con3 .service .top:before{content: ''; position: absolute; top: 0; left: 0; width: 100%; height: 100%;}
.con3 .service .top:hover:before{background: linear-gradient(to bottom, rgba(17,188,203,0.1) 0%, rgba(17,188,203,0.5) 100%);}
.con3 .service:nth-of-type(2) .top{background-image: url('/Public/images/index/pic6.jpg')}
.con3 .service:nth-of-type(3) .top{background-image: url('/Public/images/index/pic7.jpg')}
.con3 .service:nth-of-type(4) .top{background-image: url('/Public/images/index/pic8.jpg')}
.con3 .service:nth-of-type(5) .top{background-image: url('/Public/images/index/pic9.jpg')}
.con3 .service .top p.desc,.con3 .service .top i,.con3 .service .top p.title{transition: transform .35s; transform: translate3d(0, 0, 0);}
.con3 .service .top:hover p.desc,.con3 .service .top:hover i,.con3 .service .top:hover p.title{transition: transform .35s; transform: translate3d(0, -10px, 0);}
.con3 .service .top:hover a{opacity: 1; transition: opacity .35s;}
.con3 .service .bottom li:hover{color: #077478;}
.con3 .service .bottom li i{position: absolute; top: 0; bottom: 0; left: 6px; margin: auto; display: inline-block; width: 4px; height: 4px; background: #bbbbbb;}
.con4{height: 642px;}
.con4 .infor_left{position: relative; width: 680px; height: 300px; background: url('/Public/images/index/pic10.jpg') no-repeat center; background-size: 100%; text-align: center; color: #ffffff;}
.con4 .infor_left:before,.con4 .infor_left:after{content: ''; position: absolute; top: 20px; left: 20px; width: 640px; height: 260px; box-sizing: border-box; z-index: 1;}
.con4 .infor_left:before{border-top: 1px solid #ffffff; border-bottom: 1px solid #ffffff; transform: scale(0,1); transition: opacity .35s, transform .35s;}
.con4 .infor_left:after{border-left: 1px solid #ffffff; border-right: 1px solid #ffffff; transform: scale(1,0); transition: opacity .35s, transform .35s;}
.con4 .infor_left:hover:before,.con4 .infor_left:hover:after{transition: opacity .35s, transform .35s; opacity: 1; transform: scale(1);}
.con4 .infor_left p.title{margin-top: 120px; font-size: 30px;}
.con4 .infor_left p.desc{font-size: 16px;}
.con4 .infor_left a{position: absolute; left: 0; right: 0; bottom: 28px; margin: auto; display: inline-block; font-size: 12px; color: #ffffff; text-decoration: underline; z-index: 999;}
.con4 .infor_left p.desc,.con4 .infor_left a{opacity: 0; transition: opacity .35s, transform .35s; }
.con4 .infor_left:hover p.title{transition: opacity .35s, transform .35s; opacity: 1; transform: translate3d(0, 1px, 0);}
.con4 .infor_left:hover p.desc{transition: opacity .35s, transform .35s; opacity: 1; transform: translate3d(0, 16px, 0);}
.con4 .infor_left:hover a{transition: opacity .35s, transform .35s; opacity: 1; transform: translate3d(0, -16px, 0);}
.con4 .infor_left i{position: absolute; top: 0; left: 0; width: 100%; height: 100%;}
.con4 .infor_left:hover i{background: linear-gradient(to bottom, rgba(17,188,203,0.1) 0%, rgba(17,188,203,0.5) 100%);}
.con4 .infor_right{width: 490px; height: 300px;}
.con4 .infor_right dl{width: 100%; height: 110px;}
.con4 .infor_right dl dt{width: 145px; height: 100%; float: left;}
.con4 .infor_right dl dt img{width: 100%; height: auto;}
.con4 .infor_right dl dd{padding: 14px 26px 0 14px; width: 345px; height: 100%; background: #35b3b8; float: left; color: #ffffff; box-sizing: border-box;}
.con4 .infor_right dl dd p.title{font-size: 18px;}
.con4 .infor_right dl dd p.desc{margin-top: 12px; line-height: 22px; font-size: 14px;}
.con4 .infor_right ul{margin-top: 30px; width: 100%;}
.con4 .infor_right ul li{position: relative; margin-bottom: 15px; padding-left: 14px; line-height: 14px; font-size: 14px; color: #666666;}
.con4 .infor_right ul li:hover{color: #35b3b8;}
.con4 .infor_right ul li i{position: absolute; top: 0; left: 0; display: inline-block; width: 2px; height: 14px; background: #cccccc;}
.con5{height: 564px; background: #f5f5f5;}
.con5 ul li{width: 252px; margin-right: 64px; float: left;}
.con5 ul li:nth-child(4){margin-right: 0;}
.con5 ul li img{width: 100%; height: auto;}
.con5 ul li p.title{line-height: 44px; text-align: center; font-size: 14px; color: #666666;}
