.swiper-container{position: relative; width: 100%; height: auto; min-width: 1200px;}
.swiper-wrapper{width: 100%; height: auto;}
.swiper-slide,.swiper-slide img{width: 100%; height: auto;}
.swiper-pagination.swiper-pagination-bullets{position: absolute; bottom: 36px; z-index: 999;}
.swiper-pagination8{z-index: 999;}
h2{margin: 90px 0 70px;}
h2 p{width: 100%; text-align: center;}
h2 p.title{font-size: 30px; color: #333333; }
h2 p.desc{margin-top: 8px; font-size: 18px; color: #999999;}
.toTop{position: fixed; bottom: 40px; right: 10px; width: 40px; height: 40px; cursor: pointer; overflow: hidden;}
.toTop img{transition: transform .35s; display: block;}
.toTop:hover img{transform: translate3d(0, -40px, 0); transition: transform .35s;}
.banner .swiper-pagination span{position: relative; display: inline-block; margin-right: 6px; width: 30px; height: 4px; background: #ffffff; opacity: 1; cursor: pointer;}
.banner .swiper-pagination span strong{position: absolute; left: 0; top: 0; display: inline-block; width: 0; height: 4px; background: #077478; z-index: 999;}
.banner .swiper-pagination span.active strong{width: 31px; transition: width 3.2s;}
.banner .w1200{position: absolute; top: 0; left: 0; right: 0; margin: auto; height: 100%;}
.banner .swiper-button-prev{display: none; width: 40px; height: 70px; background-image: url(/Public/images/index/left.png); background-size: 100%;}
.banner .swiper-button-prev.active{display: block;}
.banner .swiper-button-next{display: none; width: 40px; height: 70px; background-image: url(/Public/images/index/right.png); background-size: 100%;}
.banner .swiper-button-next.active{display: block;}
.con{width: 100%; overflow: hidden; min-width: 1200px;}
.con1{height: 892px;}
.con1 .w1200{position: relative;}
.con1 h2{margin-bottom: 0;}
.con1 .tab{width: 900px; height: 72px; margin: 0 auto; border-bottom: 2px solid #077478; line-height: 72px; font-size: 16px; color: #666666; box-sizing: border-box;}
.con1 .tab li{position: relative; padding: 0 25px; float: left; cursor: pointer;}
.con1 .tab li i{display: none; position: absolute; left: 0; right: 0; bottom: -20px; margin: auto; width: 0; height: 0; border-width: 10px; border-style: solid; border-color: #077478 transparent transparent transparent;}
.con1 .tab li.active i{display: block;}
/*.con1 .swiper-container0{position: absolute; top: 184px; left: 0; background: #ffffff;}*/
.con1 .swiper-container0{position: absolute; top: 130px; left: 0; background: #ffffff;}
.con1 .swiper-container0{z-index: 1;}
.con1 .swiper-container0.active{z-index: 999;}
