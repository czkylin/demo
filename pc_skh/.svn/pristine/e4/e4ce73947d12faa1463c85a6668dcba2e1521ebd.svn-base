.con1 .swiper-button-next0{position: absolute; width: 40px; height: 70px; right: 0px; top: 160px; margin: auto; background: url('/Public/images/index/right1.png') no-repeat center; background-size: 100%; z-index: 999;}
.con1 .swiper-button-next0:hover{background-image: url('/Public/images/index/right2.png')}
.con2{height: 592px; padding-top: 90px; background: #0c3e40; overflow: inherit; box-sizing: border-box;}
.con2 h2{overflow: hidden; margin-top: 0;}
.con2 h2 p.title{color: #ffffff;}
.con2 h2 p.desc{color: #999999;}
.con2 .picContent{position: relative; width: 300px; height: 420px; float: left; text-align: center; background: url('/Public/images/index/pic1.jpg') no-repeat center; background-size: 100%; z-index: 999;}
.con2 a:nth-of-type(2) .picContent{background-image: url('/Public/images/index/pic2.jpg');}
.con2 a:nth-of-type(3) .picContent{background-image: url('/Public/images/index/pic3.jpg');}
.con2 a:nth-of-type(4) .picContent{background-image: url('/Public/images/index/pic4.jpg');}
.con2 .picContent:before{content: ''; position: absolute; width: 100%; height: 100%; top: 0; left: 0;}
.con2 .picContent img{margin: 90px 0 36px; /**/transition: transform 0.35s; transform: translate3d(0, 0, 0);}
.con2 .picContent i{display: block; margin: 0 auto; width: 18px; height: 2px; background: #ffffff; opacity: 1; transition: opacity .35s, transform .35s; transform: translate3d(0, 0, 0);}
.con2 .picContent p.title{margin-top: 36px; font-size: 18px; color: #ffffff; transition: transform .35s; transform: translate3d(0, 0, 0);}
.con2 .picContent p.text{opacity: 0; margin-top: 24px; padding: 0 26px; line-height: 26px; font-size: 14px; color: #ffffff; transition: opacity .35s, transform .35s;  transform: translate3d(0, 0, 0);}
.con2 .picContent.active img{transition: transform 0.35s; transform: translate3d(0, -36px, 0);}
.con2 .picContent.active i{opacity: 0; transition: opacity .35s, transform .35s; transform: translate3d(0, -72px, 0);}
.con2 .picContent.active p.title{transition: transform .35s; transform: translate3d(0, -72px, 0);}
.con2 .picContent.active p.text{opacity: 1; transition: opacity .35s, transform .35s;  transform: translate3d(0, -72px, 0);}
.con2 .picContent.active:before{background: linear-gradient(to top, rgba(17,188,203,0.1) 0%, rgba(17,188,203,0.6) 75%);}
