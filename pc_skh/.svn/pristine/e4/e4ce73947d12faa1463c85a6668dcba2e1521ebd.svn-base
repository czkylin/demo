.con1 .swiper-slide dl:nth-child(1){margin-left: 30px;}
.con1 .swiper-slide dl:nth-child(3){margin-right: 30px;}
.con1 .swiper-slide dl img{width: 150px; height: auto;}
.con1 .swiper-slide dl p.title{margin: 20px 0 10px 0; font-size: 20px; color: #333333; font-weight: bold;}
.con1 .swiper-slide dl p.desc{font-size: 14px; color: #666666;}
.con1 .swiper-slide dl p.text{width: 280px; margin: 22px auto 0; line-height: 22px; font-size: 14px; color: #333333;}
.con1 .swiper-slide dl a{position: absolute; bottom: -20px; left: 0; right: 0; margin: auto; display: inline-block; width: 120px; height: 40px; line-height: 40px; background: #f7922e; font-size: 14px; color: #ffffff; border-radius: 10px;}
