/* reset css */
*{margin: 0; padding: 0;}
img{vertical-align: top;}
a{-webkit-tap-highligh-color:rgba(0,0,0,0); text-decoration: none;}
html,body{height: 100%; overflow: hidden; }
li{list-style: none;}
/* commom css*/
body{position: relative;}
@-webkit-keyframes move{
	0%{
		-webkit-transform:translateY(10px);
		transform:translateY(10px);
	}
	50%{
		-webkit-transform:translateY(0px);
		transform:translateY(0px);
	}
	100%{
		-webkit-transform:translateY(10px);
		transform:translateY(10px);
	}
}
@keyframes move{
	0%{
		-webkit-transform:translateY(10px);
		transform:translateY(10px);
	}
	50%{
		-webkit-transform:translateY(0px);
		transform:translateY(0px);
	}
	100%{
		-webkit-transform:translateY(10px);
		transform:translateY(10px);
	}
}
.arrow{
	-webkit-animation: move 1s linear infinite;
	animation: move 1s linear infinite;
	position: absolute;height: 0.66rem;display: block;width: 100%;left: 0;bottom: 2.2rem;
    background: url(/weixin/Public/Member/images/huzhu/share_des/arrow.png) no-repeat center;
    background-size: 0.78rem 0.66rem;
    z-index: 5;
}
/* main css */
/* page1 */
.page1{}
.page1 .title{display: block; width: 12.62rem; margin: 0.5rem auto 0;}
.page1 .des1{display: block; width: 12.54rem; margin: 0.2rem auto 0; opacity: 0}
.page1 .des2{display: block; width: 12.64rem; margin: 0.2rem auto 0; opacity: 0}
.page1 .img{display: block; width: 11.78rem;margin:1.7rem auto 0;}

.page1.animate .des1{-webkit-animation:fadeInDown 0.5s 0.2s forwards;animation:fadeInDown 0.5s 0.2s forwards;}
.page1.animate .des2{-webkit-animation:fadeInDown 0.5s 0.2s forwards;animation:fadeInDown 0.5s 0.2s forwards;}
/* page2 */
.page2 .title{display: block; width: 13.16rem; margin: 0.5rem auto 0;}
.page2 .des{display: block; width: 6.44rem; margin: 1.2rem auto 0.86rem;}
.page2 .con{display: block; width: 13.52rem; margin: 0 auto; opacity: 0}
.page2 .img{display: block; width: 6.4rem; margin:0.6rem auto 0;}

.page2.animate .con{-webkit-animation:fadeIn 0.5s 0.2s forwards;animation:fadeIn 0.5s 0.2s forwards;}
/* page3 */
.page3 .title{display: block; width: 13.64rem; margin: 0.5rem auto 0;}
.page3 .des1{display: block; width: 8.66rem; margin: 1.2rem auto 0.6rem;}
.page3 .des2{display: block; width: 11.8rem; margin: 0 auto;}
.page3 .con{display: block; width: 8.74rem; margin: 1.3rem auto 0.7rem;}
.page3 .img{display: block; width: 12.8rem; margin:0 auto;}

.page3.animate .con{-webkit-animation:pulse 0.5s 0.2s forwards;animation:pulse 0.5s 0.2s forwards;}

/* page4 */
.page4 .title{display: block; width: 13.36rem; margin: 0.5rem auto 1.4rem;}
.page4 .con{width: 13.2rem; height: 16.8rem; margin:0 auto; overflow: hidden; position: relative;}
.page4 .swiper-slide img{display: block; width: 12.4rem; margin: 0 auto;}
.page4 .swiper-pagination{bottom: 0;}
.page4 .swiper-pagination-bullet-active{background: #93b77c;}

/* page5 */
.page5 .title{display: block; width: 13.84rem; margin: 0.5rem auto 0.5rem;}
.page5 .des{display: block; width: 100%; opacity: 0;}
.page5 .img{display: block; width: 11.96rem; margin:1rem auto 0;}

.page5.animate .des{-webkit-animation:bounceIn 0.5s 0.2s forwards;animation:bounceIn 0.5s 0.2s forwards;}

/* page6 */
.page6 .title{display: block; width: 13.18rem; margin: 0.5rem auto 1rem;}
.page6 .img1{display: block; width: 100%; opacity: 0;}
.page6 .img2{display: block; width: 100%; opacity: 0;}
.page6 .des{display: block; width: 9.24rem; margin: 1.64rem auto 0; opacity: 0;}
.page6.animate .img1{-webkit-animation:fadeInLeft 0.5s 0.4s forwards;animation:fadeInLeft 0.5s 0.4s forwards;}
.page6.animate .img2{-webkit-animation:fadeInRight 0.5s 0.9s forwards;animation:fadeInRight 0.5s 0.9s forwards;}
.page6.animate .des{-webkit-animation:fadeIn 0.5s 1.4s forwards;animation:fadeIn 0.5s 1.4s forwards;}

/* page7 */
.page7 .title{display: block; width: 13.18rem; margin: 0.5rem auto 0.5rem;}
.page7 .img{display: block; width: 100%;}

/* page8 */
.page8 .title{display: block; width: 13.18rem; margin: 0.5rem auto 0.5rem;}
.page8 .img{display: block; width: 14.04rem; margin: 0 auto;}

.link{position: absolute; left: 0; bottom: 0; display: block; width: 100%; height: 1.8rem;line-height: 1.8rem; text-align: center; background: #ff8400; color: #fff; font-size: 0.8em; z-index: 10;}

.fl{float: left;}
.nav{position:fixed;top:0;left:0;background:rgba(0,0,0,0.3);width:13.75rem;padding:10px 0.625rem; z-index: 10000;}
.nav .people{overflow:hidden;position:relative;width:10rem;height:18px;}
.nav ul{height:18px;position:absolute;top:0;left:0;}
.nav span{color:#f70a0a;display:inline-block;width:3.75rem;font-size:16px;}
.nav em{color:#fff;font-style:normal;margin:0 10px;display:block; font-size: 16px;}