html,body{width: 100%; height: 100%;}
.swiper-container{width: 100%; height: 100%;}
.swiper-slide{background: url(/weixin/Public/Member/images/aid_neimeng/bg3.jpg) no-repeat left top; background-size: 100% 100%; width: 100%; height: 100%; position: relative;}
img{position: absolute; z-index: 2;}
img.bgBottomLeft{width: 56.7%; bottom: 0; left: 0;}
img.bgRectangleBig{width: 97.6%; height: 93.6%; top: 3.2%; left: 1.2%;}
.arrow{width:24px;height:24px;position:absolute;bottom:2rem;left:50%;margin-left:-12px;z-index:100;}
.arrow:before{position:absolute;top:0;left:0;width:20px;height:20px;border:2px solid #11783a;border:2px solid #fff;border-bottom-color:rgba(0,0,0,0);border-right-color:rgba(0,0,0,0);border-radius:4px;content:'';transform:rotate(45deg);-webkit-transform:rotate(45deg);}
@-webkit-keyframes rise{
    0% {
        opacity:0;
        -webkit-transform: translate3d(0,100px,0);
        transform: translate3d(0,100px,0)
    }
    40% {
        opacity:1;
        -webkit-transform: translate3d(0,0,0);
        transform: translate3d(0,0,0)
    }
    100% {
        opacity:0;
        -webkit-transform: translate3d(0,-20px,0);
        transform: translate3d(0,-20px,0)
    }
}

@keyframes rise{
    0% {
        opacity:0;
        -webkit-transform: translate3d(0,100px,0);
        transform: translate3d(0,100px,0)
    }
    40% {
        opacity:1;
        -webkit-transform: translate3d(0,0,0);
        transform: translate3d(0,0,0)
    }
    100% {
        opacity:0;
        -webkit-transform: translate3d(0,-20px,0);
        transform: translate3d(0,-20px,0)
    }
}

.rise{
    -webkit-animation-name: rise;
    animation-name: rise;
    -webkit-animation-iteration-count:infinite;
    animation-iteration-count:infinite;
}

