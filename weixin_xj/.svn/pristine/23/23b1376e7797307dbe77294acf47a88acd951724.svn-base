.txt { width: 100%; height: 60px; background-color: #fff; border-bottom: 1px solid #e5e5e5; /*padding-top: 4.25rem;*/ overflow: hidden; }
.txt textarea { width: 100%; height: 60px; line-height: 20px; font-size: 12px; color: #999; border: 0; padding: 15px; }
.uploader { min-height: 50px; width: 100%; background-color: #fff; position: relative; margin-bottom: 12px; padding: 10px 10px 10px 65px; color: #999; font-size: 10px; -webkit-box-sizing: border-box; box-sizing: border-box; }
#pre_pic { display: none; }
.uploader .g-image-upload-box .upload-btn { display: block; width: 32.5px; height: 40px; background: url(../../images/cf/icon_img.jpg) no-repeat left center; background-size: 100%; position: absolute; left: 0; top: 5px; }
.g-image-upload-box .preview.small, .g-image-upload-box .upload-btn.small { border: 0; }
.J_Showmeonfileuploaded { color: #999; }
.g-image-upload-box .upload-btn.small .file-input { font-size: 4.5rem; height: 45px; width: 45px; }
.img { height: 50px; width: 100%; background-color: #fff; position: relative; margin-bottom: 12px; padding: 10px 10px 10px 65px; color: #999; font-size: 10px; -webkit-box-sizing: border-box; box-sizing: border-box; }
.img a { display: block; width: 32.5px; height: 40px; background-size: 32.5px; background: url(../../images/cf/icon_img.jpg) no-repeat left center; position: absolute; left: 15px; top: 5px; }
.img b { font-size: 11px; font-weight: normal; }
.chanpin { width: 100%; padding: 15px; background-color: #fff; -webkit-box-sizing: border-box; box-sizing: border-box; }
.chanpin h2 { font-size: 12px; color: #333; margin-bottom: 15px; font-weight: normal }

