*{margin:0;padding:0;-webkit-tap-highlight-color:rgba(0,0,0,0);vertical-align:baseline;}
body{font-family:"microsoft Yahei";font-size:16px;}
li{list-style:none;}
img{border:none;vertical-align:middle;}
.page{width:100%;}
.page section{width:90%;height:80px;margin:0 auto;display:box;display:-webkit-box;padding:20px 0;overflow:hidden;border-bottom:1px dotted #d7d7d7;position:relative;}
.page section a{position:absolute;top:0;left:0;width:100%;height:100%;}
.text{width:68%;height:auto;}
.img{width:32%;overflow:hidden;background:#e8e8e8 url(/weixin/Application/Member/View/images/zhengcejiedu/logo.png) no-repeat center center;background-size:39px;}
.img img{width:100%;display:box;display:-webkit-box;}
.text h2{font-size:16px;font-weight:normal;line-height:20px;width:90%;height:40px;overflow:hidden;}
.text p{display:box;display:-webkit-box;margin-top:20px;}
.text strong,.text em{font-size:12px;font-weight:normal;font-style:normal;color:#666;margin-right:1em;display:block;}
.more{width:100%;text-align:center;color:#efefef;height:60px;line-height:60px;font-size:16px;}

@media all and (min-width:900px){
    .page{width:900px;margin:0 auto;}
    .page section{width:90%;height:200px;padding:20px 0;}
    .text{width:70%;}
    .img{width:30%;}
}