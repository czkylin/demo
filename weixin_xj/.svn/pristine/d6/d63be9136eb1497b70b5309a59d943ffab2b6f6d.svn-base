body{background:#fff;color:#333;font-size:16px;-webkit-tap-highlight-color:transparent;}
input{-webkit-appearance:none;outline:none;}
.unit{padding-top:1rem;}
.unit p:first-child{padding-bottom:.5rem;}
.sec:nth-child(2) .unit p:first-child{padding-bottom:0;}
.sec{padding-bottom:1rem;padding:0 0.5rem 1rem;}
.bt{border-bottom:5px solid #efefef;}
.tel{color:#0178de;font-size:16px;}
.startBtn{color:#fff;background:#ee4a65;border-radius:3px;display:inline-block;width:5rem;line-height:40px;margin-top:1rem;}
.flow{font-size:14px;color:#9a9a9a;width:11.2rem;padding:.5rem .4rem;}
.flow em{font-style:normal;}
.flow em.active{color:#ef4866;}
.expert{width:8rem;margin:1rem auto 0;border:1px dashed #9a9a9a;color:#9a9a9a;height:6rem;padding:0 .5rem;}
span.active{color:#ee4a65;font-size:20px;}
.text{line-height:24px;border-width:0;font-size:16px;color:#999;padding:0 0.2rem;width:6rem;}
.phone{border:1px solid #ccc;border-radius:3px;line-height:30px;height:30px;padding:0 10px;color:#999;margin-left:10px;}
.section label{position:relative;display:inline-block;}
.file{position:absolute;top:0;left:-1000px;}
.section li{border-bottom:1px solid #efefef;padding:.5rem;font-size:14px;}
.section li.other{border-bottom-width:0;color:#999;padding-bottom:0;}
.section li.special{border-bottom-width:0;padding-bottom:0;}
.section li span{display:inline-block;line-height:24px;}
.section li i{background:url(/weixin/Public/Member/images/apply/picicon.png) no-repeat;background-size:cover;width:2rem;height:1.37rem;display:inline-block;vertical-align:middle;margin:0 10px;}
.section li b{color:#999;font-weight:normal;}
.section small{color:#e60a2b;display:inline-block;}
.go{color:#fff;background:#ef4a65;width:11rem;margin:1rem auto;display:block;line-height:40px;text-align:center;border-radius:3px;}
.grey{background:#ccc;}