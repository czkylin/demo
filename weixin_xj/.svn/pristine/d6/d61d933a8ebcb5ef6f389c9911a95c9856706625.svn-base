*{
	padding: 0;
	margin: 0;
}
img{
	vertical-align: top;
}
body{
	background: #fff;
	width: 100%;
	height: 100%;
}
a{
	text-decoration: none;
}
.img{
	text-align: center;
	width: 100%;
	margin-top: 45%;
	margin-bottom: 10px;
}
.img img{
	width: 24%;
}
p{
	font-size: 16px;
	text-align: center;
	color: #666;
	font-weight: 500;
}
.btn1,.btn2{
	position: absolute;
	left: 0;
	bottom: 20px;
}
.btn1{
	bottom: 80px;
}