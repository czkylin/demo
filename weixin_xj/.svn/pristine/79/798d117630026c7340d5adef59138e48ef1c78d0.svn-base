.name_bot{float: left; width: 75%;}
.name_bot p{color: #fff;}
.userinfo{}
.userinfo .xingming{ float: left; margin-right: 10px; color: #fff; line-height: 25px;}
.userinfo .bumen{float: left; color: #fff; line-height: 25px;}
.jifen{}
.jifen p{float: left; color: #fff; line-height: 25px; margin-right: 20px;}
.oneself{ margin-bottom:10px; border-top: 1px solid #eee;}

.Tab_CaiDan { width: 100%; height: 40px;}
.Tab_CaiDan a { width: 50%; display: block; border: 1px solid #006633; -webkit-box-sizing: border-box; box-sizing: border-box; float: left; height: 38px; line-height: 38px; text-align: center; color: #333333; font-size: 14px; }
.Tab_CaiDan a:last-child { border-left: 0; }
.Tab_CaiDan .curr { background: #006633; color: #fff; }
.hot li { position: relative; width: 100%; border-bottom: 1px solid #eee; padding: 10px 0; background:#fff; margin-bottom: 5px;}
.hot .ww { width: 35px; left: 0px; top: 18px; text-align: center; color: #999; font-size: 22px;font-family: "微软雅黑"; font-weight: 500; color: #333; }
.hot .ww img { display: block; width: 25px; height: 25px; margin: 0 auto; }
.hot .touxiang { width: 35px; height: 35px; left: 40px; top: 10px; }
.hot .touxiang a img { width: 40px; height: 40px; border-radius: 50%; }
.hot .huoyuedu { width: 100%; height: 40px; padding-left: 85px; padding-right: 10px; -webkit-box-sizing: border-box; box-sizing: border-box; }
.hot .huoyuedu .title { width: 100%; height: 22px; font-size: 16px; color: #333; }
.hot .huoyuedu .title-des{width: 55%;}
.hot .huoyuedu .title span, .hot .huoyuedu .title a { display: inline-block; }
.hot .huoyuedu .title span { font-size: 14px;font-family: "微软雅黑"; max-height: 40px; line-height: 20px; font-weight: 500; display: block;}
.oneself .huoyuedu .title span { font-size: 14px; font-family: "微软雅黑"; height: 20px; line-height: 20px; margin-top:0; }
.hot .huoyuedu .title span b { font-size: 12px; height: 20px; line-height: 20px; color: #999999; font-weight: normal; }
.hot .huoyuedu .title span.fr { color: #333; font-size: 12px; width: 45%; text-align: right; 
    overflow : hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}
.hot .huoyuedu .title span.huadong_hos{ min-height: 1em; }
.hot .huoyuedu .title a { font-size: 12px;  height: 22px; line-height: 22px; color: #999; width: 90%; overflow:hidden; display: block; text-overflow:ellipsis;white-space:nowrap;}
@media screen and (min-width: 375px) {
.hot .huoyuedu .title a { margin-left: 10px; font-size: 12px; height: 22px; line-height: 22px; color: #999; width: 65%; margin-top: 10px;overflow:hidden; }
} 
@media screen and (min-width: 400px) {
.hot .huoyuedu .title a { margin-left: 10px; font-size: 12px; height: 22px; line-height: 22px; color: #999; width: 68%; margin-top: 10px;overflow:hidden; }
} 
.hot .huoyuedu div.pr { width: 100%; height: 8px; background: #dbdbdb; margin-top: 10px; border-radius: 6px; }
.hot .huoyuedu div.pr .pa { left: 0; height: 8px; top: 0; z-index: 3; background: #ffde5d; border-radius: 6px; }
.gslmLabel .labelCon { width: 91%; height: 43px; float: left; overflow-x: scroll; overflow-y: hidden; }
.gslmLabel .labelCon a { height: 30px; line-height: 30px; float: left; margin-right: 10px; margin-top: 5px; color: #333; padding: 0 5px; }
.gslmLabel .labelCon a.curr { background: #006633; color: #fff; }
.foot_logo { position:fixed; bottom: 0; left: 0; background: #f5f5f5; height: 60px; width: 100%; }
.foot_logo span { width: 200px; margin: 12px auto; display: block; }
/*月份选择*/
.xuanze{width: 70%; height: 32px; padding: 15px 15% 8px; background:#fff;}
.xuanze .mouths{ float: left; height: 30px; border: 1px solid #ef4a65; line-height: 30px; text-align: center; border-radius: 6px; font-size: 12px; color: #ef4a65; width: 43%; position: relative; z-index: 25;}
.xuanze .mouths-curr{position: relative; z-index: 25;}
.xuanze .mouths-curr b{font-weight: normal;}
.xuanze .mouths-curr i{ margin-left: 2px; display: inline-block; width: 13px; height: 7px; background: url(/weixin/Public/Home/images/icon/down1.png) no-repeat; background-size: 13px;}
.xuanze .mouths-curr.active i{background-image: url(/weixin/Public/Home/images/icon/down2.png);}
.xuanze .mouths-curr.curr i{background-image: url(/weixin/Public/Home/images/icon/down3.png);}
.mouths-select{ display: none; position: absolute; top: 42px; right: 0; width: 125%; z-index: 22;}
.mouths-select b{position: absolute; top: -5px; right: 5px; width: 8px; height: 5px; background: url(/weixin/Public/Home/images/icon/sanjiao.png) no-repeat; background-size: 8px;}
.mouths-select ul{background: #fff; border-radius: 3px;}
.mouths-select ul li{ background:url(/weixin/Public/Home/images/icon/huanzhe1.png) no-repeat 4px center; background-size: 5px; text-align: left; text-indent: 1em; width: 100%; height: 30px; line-height: 30px; font-size: 12px; color: #333; border-radius: 6px;}
.xuanze .zongpai{ float: right; height: 30px; border: 1px solid #ef4a65; line-height: 30px; text-align: center; border-radius: 6px; font-size: 12px; color: #ef4a65; width: 33%;}
.xuanze .zongpai.active{color: #cdcbcb; border-color: #cdcbcb;}
.xuanze .mouths-curr b{font-weight: normal;}
.xuanze .mouths-curr.active{color: #ff8400;}
.xuanze .mouths-curr.curr{color: #cdcbcb;}
.xuanze .mouths.active{border-color: #ff8400;}
.xuanze .mouths.curr{border-color: #cdcbcb;}
.select-bg{display: none; width: 100%; height: 100%; position: absolute; left: 0; top: 0; background: rgba(0,0,0,0.2); z-index: 20;}
/*规则弹窗*/
.rule{
	position: absolute;
	left: 0;
	top: 0;
	height: 100%;
	width: 100%;
	background: rgba(0,0,0,0.35);
	display: none;
}
.rule-content{
	width: 82%;
	padding: 10px 2% 20px;
	background: #fff;
	border-radius: 15px;
	position: fixed;
	top: 25%;
	left: 7%;
}
.rule-close{
	position: absolute;
	width: 16px;
	height: 16px;
	right: 10px;
	top: 10px;
	background: url(/weixin/Public/Home/images/icon/close.png) no-repeat;
	background-size: 16px;
}
.rule-content img{
	display: block;
	width:38%;
	margin: 0 auto 10px; 
}
.rule-main{}
.rule-main h3{
	font-size: 14px;
	color: #ef4a65;
	line-height: 24px;
}
.rule-main ul{}
.rule-main ul li{
	font-size: 12px;
	color: #333;
	line-height: 20px;
}
