/*排行榜*/
.header{height: 20px; line-height: 20px; font-size: 12px; width: 96%; padding: 0 2% 10px; background: #fff;}
.header span{float: left; color: #999;}
.header a{float: right; color: #ef4a65;}
.PaiHang{ width: 100%; overflow: hidden; background: -webkit-linear-gradient(top left,#f7ab6f,#dc5290); background: -linear-gradient(top left,#f7ab6f,#dc5290);}
.headPaiHang{ width:96%; padding: 20px 2%; position: relative;}
.zhixing{position: absolute; display: block; right:-10%; bottom: 10%; height: 68%;}
.zhixing img{height: 100%;}
.champion{ width: 17%; position:relative; float: left; margin-right: 2%; padding-top: 17%; overflow: hidden;}
.champion a{ display: inline-block; position: absolute; top: 0; left: 0; right: 0; bottom: 0; }
.champion a img{border-radius:100%; width: 100%; position:absolute; min-height: 100%;}
