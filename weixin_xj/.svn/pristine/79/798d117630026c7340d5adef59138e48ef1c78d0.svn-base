@charset "utf-8";
button, input[type=button], input[type=file], input[type=submit] { cursor: pointer; -webkit-appearance: none }

/* 竖屏 */
@media screen and (orientation:portrait) {
.ui-mobile, .ui-mobile .ui-page { min-height: 420px }
}

/* 横屏 */
@media screen and (orientation:landscape) {
.ui-mobile, .ui-mobile .ui-page { min-height: 300px }
}
body { font-family: "微软雅黑", Arial, Helvetica, sans-serif; color: #000; margin: 0; color: #fff; font-size: 14px; background:#f0eff5; }
* { margin: 0; padding: 0 }
ul, li, h1, h2, h3, h4, sapn, dl, dd, dr, dt, i, em, p, img { list-style: none; margin: 0; padding: 0; font-style: normal; text-decoration: none;}
a { text-decoration: none; color: #333; }
a:hover { text-decoration: none; }
.ui-content { border-width: 0; overflow: visible; overflow-x: hidden; padding: 15px }
.pad_top { padding-top: 0; }
dl, dt, dd { padding: 0; margin: 0; }
h1, h2, h3, h4 { margin: 0; padding: 0; }
.ffw { font-family: "微软雅黑" }
.ffa { font-family: Arial, Helvetica, sans-serif }
.ffs { font-family: "宋体"; }
.fl { float: left }
.fr { float: right }
.pr { position: relative }
.pa { position: absolute }
.pf { position: fixed; }
.tac { text-align: center; }
.tal { text-align: left }
.tar { text-align: right }
.fwb { font-weight: bold }
.fw1 { font-weight: 100 }
.fs14 { font-size: 14px; }
