body{background:#ecf8ff;font-size:16px;}
html,body{height:100%;}
body.blues{background:#beeffc;}
input{outline:none;-webkit-appearance:none;border:1px solid #69c7e9;background:none;}
input:focus{box-shadow:0 0 10px #69c7e9;}
::-webkit-input-placeholder{color:#0080ae;}
.label{box-sizing:border-box;-webkit-box-sizing:border-box;}
.label strong{color:#f00;font-weight:normal;margin-right:.1rem;font-size:12px;}
.forms{width:100%;/*margin:-1px 1rem;*/border-radius:4px;/*padding:1rem;*/background:url(/weixin/Public/Member/images/100card/line.jpg) repeat-y center 0;background-size:contain;}
.forms form{width:7rem;margin:0 2.2rem;}
.forms span{border-top-left-radius:4px;border-bottom-left-radius:4px;color:#0080ae;display:block;width:1.2rem;line-height:30px;height:30px;border:1px solid #69c7e9;background:url(/weixin/Public/Member/images/100card/icons.png) no-repeat center 0 #69c7e9;background-size:.8rem 100px;}
.label:nth-of-type(3) span,.label:nth-of-type(7) span{background-position:center -34px;}
.label:nth-of-type(5) span{background-position:center -68px;}
.forms input{border-top-right-radius:4px;border-bottom-right-radius:4px;width:6.4rem;display:block;padding:0 .2rem;color:#0080ae;line-height:30px;height:30px; font-size: 16px;background:#ecf8ff;}
.forms a{background:#3ea0b9;border-radius:4px;line-height:35px;width:40%;color:#fff;font-size:18px;margin:0 auto;display:block;margin-top:10px;}
.info{line-height:28px;height:28px;}
.info i{font-size:14px;color:#f70a0a;font-style:normal;}
.more a{display:block;width:40%;color:#0080ae;border:1px solid #0080ae;border-radius:6px;height:40px;line-height:40px;margin:20px auto 100px;}
footer{position:fixed;bottom:0;left:0;width:100%; display: flex; display: -webkit-flex;}
footer span{flex:0 0 100px; -webkit-flex:0 0 100px; width:100px; text-align: center; line-height: 50px; background: #ff8400; color:#fff;}
footer a{color:#fff;line-height:50px;display:block; font-size:20px;background:#bf1b20; flex:1; -webkit-flex:1;}
/* footer a:nth-child(1){background:#1dc0be;}
footer a:nth-child(2){background:#e76814;} */
article{overflow:hidden;padding-top:50px;}
.mask{background:rgba(0,0,0,0.5);position:fixed;top:0;left:0;width:100%;height:100%;display:none;}
.mask .dialog{position:absolute;top:0;left:0;width:100%;height:100%;}
.alert{background:#fff;border-radius:6px;width:80%;}
.top{padding:10px;}
.top h2{font-size:16px;padding-bottom:10px;}
.bottom{box-sizing:border-box;-webkit-box-sizing:border-box;border-top:1px solid #ccc;}
.bottom a{color:#1e90ff;display:block;width:50%;height:30px;line-height:30px;}
.bottom a:last-child{border-left:1px solid #ccc;}
.nav{position:fixed;top:0;left:0;background:rgba(0,0,0,0.3);width:11rem;padding:10px 0.5rem; z-index: 20;}
.nav .people{overflow:hidden;position:relative;width:8rem;height:18px;}
.nav ul{height:18px;position:absolute;top:0;left:0;}
.nav span{color:#f70a0a;display:inline-block;width:3rem;}
.nav em{color:#fff;font-style:normal;margin:0 10px;display:block;}
.guarantee-faq{width:90%;padding:0 5%;}
.item{width:100%;}
.item header{line-height:40px;position:relative;padding-right:40px;}
.item header:before{position:absolute;content:'';top:50%;right:10px;width:10px;height:10px;margin-top:-10px;border-bottom:2px solid #999;border-right:2px solid #999;transform:rotate(45deg);-webkit-transform:rotate(45deg);}
.item header.active{color:#f70a0a;}
.item header.active:before{transform:rotate(225deg);margin-top:-5px;}
.q-content{display:none;background:#f6f6f6;padding:10px;font-size:14px;}
.q-content p{padding:5px;line-height:20px;}
.item:nth-of-type(1) .q-content{display:block;}
.cn{color:#666;font-size:14px;width:80%;margin:0 auto;line-height:20px;}
.cn i{display:inline-block;background:#3ea0b9;border-radius:6px;width:6px;height:6px;margin-right:5px;}
.cn a{color:#ef4a65;}
.infos{color:#f00;font-size:12px;padding:10px 0;width:64%;margin:0 auto; margin-bottom:10px; padding-left:5%;}
/*pop*/
#pop{position: fixed; left: 0; top: 0; z-index:10; height: 100%; width: 100%; display: none;}
#pop .mask{position: absolute; left: 0; top: 0; width: 100%; height: 100%; display: block; background: rgba(0,0,0,0.7);}
#pop .close{position: absolute; right: 0; top: -1.152rem; display: block; width: 1rem; height: 1rem; background: url(/weixin/Public/Member/images/huzhu/close.png) no-repeat center; background-size: 0.8rem; }
#pop .pop-content{width: 8rem; margin: 3.5rem auto; background: #fff; border-radius: 10px; position: relative; z-index: 11;}
#pop .pop-main{border-bottom: 1px solid #e7e7e7; padding: 0.8rem 0.8rem 0;}
#pop .pop-main img{width: 6.4rem; height: 6.4rem; display: block;}
#pop .pop-main p{height: 1.2rem; line-height: 1.2rem; text-align: center; font-size: 0.48rem; color: #666666;}
#pop .pop-des{height: 1.12rem; line-height: 1.12rem; text-align: center; font-size: 0.384rem; color: #999;}
/*二维码*/
#erweima{position: fixed; right: 10px; bottom: 60px; border: 1px solid #ccc; background: #fff; border-radius: 10px; padding: 12px; display: none;}
#erweima img{width: 1.312rem; height: 1.312rem; margin-bottom: 0.16rem;}
#erweima span{display: block; height: 0.576rem; line-height: 0.576rem; text-align: center; font-size: 0.416rem; color: #333333;}

.video{width: 100%; position: relative;}
.video img{width: 100%;}
.video video{position: absolute; left: 8%; top: 5px; display: block; width: 84%;}