.share-box {width: 100%;height: 100%;position: fixed;top: 0;display: none;z-index: 998;}
.share-bg{width: 100%;height: 100%;background: #000;opacity: 0.5;}
.share-box img{position: absolute;top: .2rem;right: .2rem;width: 3.3rem;height: 2.28rem;z-index: 999;}
.popup-box.noChans{width: 100%; height: 100%; background: url('../images/bj.png') no-repeat; background-size: 100% 100%;}

