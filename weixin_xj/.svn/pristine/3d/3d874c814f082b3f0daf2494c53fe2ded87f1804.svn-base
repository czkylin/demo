body,html,.swiper-container,.swiper-slide{height:100%;}
a{-webkit-tap-highlight-color:rgba(0,0,0,0);}
.art{padding:.5rem 1rem;}
.opacity0{opacity:0;}
.paint{position:relative;}
.paint img{margin-top:6rem;}
.paint i{position:absolute;background:url(/weixin/Public/Member/images/huzhu/father/heart.png) no-repeat;width:1rem;height:1rem;background-size:100% 400%;}
.paint i:nth-of-type(1){top:4rem;left:.5rem;}
.paint i:nth-of-type(2){top:4.5rem;right:.5rem;background-position:0 -1rem;}
.paint i:nth-of-type(3){bottom:-1.5rem;left:.5rem;background-position:0 -2rem;}
.paint i:nth-of-type(4){bottom:-2rem;right:.3rem;background-position:0 -3rem;}
.kill{-webkit-transform: translate3d(-12rem,0,0);transform: translate3d(-12rem,0,0)}
.divide{position:absolute;background:#fff;width:100%;height:100%;top:0;left:0;}
.action{color:#666;font-size:14px;line-height:40px;-webkit-transform: translate3d(12rem,0,0);transform: translate3d(12rem,0,0)}
.arrow{width:30px;height:30px;position:absolute;bottom:2rem;left:50%;margin-left:-12px;z-index:100;background:url(/weixin/Public/Member/images/huzhu/father/arrow.png) no-repeat;background-size:contain;}
.s1{background:url(/weixin/Public/Member/images/huzhu/father/bg1.jpg) no-repeat;background-size:100% 100%;}
.s1 p{margin-top:1rem;}
.s1 p:nth-of-type(2){margin-top:1rem;}
.s1 p:nth-of-type(3){margin-top:1rem;}
.s3{background:url(/weixin/Public/Member/images/huzhu/father/bg2.jpg) no-repeat;background-size:100% 100%;}
.titles{background:url(/weixin/Public/Member/images/huzhu/decoration/titles.png) no-repeat;background-size:100% 600%;width:10rem;height:3.4rem;position:absolute;top:.5rem;left:50%;margin-left:-5rem;}
.tis{margin-top:3rem;}
.fatherLove{position:absolute;right:0;bottom:0;width:8rem;transform:scale(2,2);-webkit-transform:scale(2,2);}
.parent{position:absolute;right:0;bottom:0;}
.care{position:absolute;left:.5rem;bottom:2rem;width:2rem;}
.s4{background:url(/weixin/Public/Member/images/huzhu/father/bg3.jpg) no-repeat;background-size:100% 100%;}
.busy{margin-top:-6rem;}
.lamp,.light{position:relative;z-index:10;}
.lamp{z-index:10;}
.light{z-index:9;}
.light img{margin-top:-0.2rem;}
.black{position:absolute;top:0;left:0;width:100%;height:100%;background:#000;}
.bg3{background-image:url(/weixin/Public/Member/images/mother/bg3.jpg);}
.bg4{background-image:url(/weixin/Public/Member/images/mother/bg4.jpg);}
.bg5{background-image:url(/weixin/Public/Member/images/mother/bg5.jpg);}
.bg6{background-image:url(/weixin/Public/Member/images/mother/bg6.jpg);}
.s4 .titles{background-position:0 -3.35rem;}
.s3 p:nth-of-type(1){color:#0273c1;font-size:16px;}
.s3 p:nth-of-type(2),.s5 p:nth-of-type(2),.s5 p:nth-of-type(4){color:#4d4e45;font-size:14px;line-height:24px;margin-top:.2rem;}
.s3 p:nth-of-type(3),.s5 p:nth-of-type(3),.s5 p:nth-of-type(5){color:#4d4e45;font-size:14px;line-height:24px;}
.s3 p:nth-of-type(4) img:nth-child(1){margin-right:-1rem;position:relative;z-index:1;}
.s4 p:nth-of-type(2){height:6.4rem;overflow:hidden;position:relative;}
.s4 p:nth-of-type(1) b{width:30rem;height:10px;background:rgba(255, 255, 255, 0.15);position:absolute;top:0;left:0;background: -webkit-linear-gradient(rgba(255,255,255,0.3),rgba(255,255,255,1),rgba(255,255,255,0.3));background: linear-gradient(rgba(255,255,255,0.3),rgba(255,255,255,1),rgba(255,255,255,0.3));transform:rotate(145deg);-webkit-transform:rotate(145deg);-webkit-transform-origin:0% 0%;transform-origin:0% 0%;}
.s5{background:url(/weixin/Public/Member/images/huzhu/father/bg4.jpg) no-repeat;background-size:100% 100%;}
.s5 .titles{background-position:0 -6.7rem;}
.s5 span{font-size:10px;color:#666;display:inline-block;width:3.3rem;overflow:hidden;vertical-align:top;}
.s5 section{padding-top:4.5rem;}
.s5 p i{font-size:16px;color:#933;}
.fs{position:absolute;right:.5rem;bottom:0;}
.s6{background:url(/weixin/Public/Member/images/huzhu/father/bg5.jpg) no-repeat;background-size:100% 100%;}
.s6 .titles{background-position:0 -10.05rem;}
.s7{background:url(/weixin/Public/Member/images/huzhu/father/bg6.jpg) no-repeat;background-size:100% 100%;}
.s7 .titles{background-position:0 -13.4rem;}
.s8{background:url(/weixin/Public/Member/images/huzhu/decoration/bg3.jpg) no-repeat;background-size:100% 100%;}
.s8 .titles{background-position:0 -16.75rem;}
.buy{position:fixed;bottom:0;left:0;width:100%;z-index:1000;transition:.5s;-webkit-transition:.5s;transform:translate3d(0,70px,0);-webkit-transform:translate3d(0,70px,0);}
.move{transform:translate3d(0,0,0);-webkit-transform:translate3d(0,0,0);}
.num{width:5rem;color:#fff;font-size:16px;height:40px;line-height:40px;text-align:center;background:#ff8400;}
.buy a{display:block;width:7rem;color:#fff;font-size:16px;height:40px;line-height:40px;text-align:center;background:#bf1b20;}
.pic{padding-top:2rem;position:relative;}
.pic i{position:absolute;background:url(/weixin/Public/Member/images/mother/print.png) no-repeat;background-size:cover;width:2rem;height:1.24rem;top:1rem;right:1rem;}
.bird{background:url(/weixin/Public/Member/images/mother/bird.png) no-repeat center 0;background-size:90% 100%;padding-top:0;margin-top:2rem;}
.bird img{margin-top:2rem;}
.bird i{top:-1rem;}
.btn{width:5rem;display:block;margin:1rem auto;}
.img-group{width:10rem;}
.imgs:nth-child(1){width:6rem;margin-top:.5rem;}
.imgs:nth-child(2){width:4rem;}
.imgs:nth-child(2) img{margin-left:1rem;}
.imgs:nth-child(3){width:10rem;margin-top:.5rem;}
.music{position:fixed;right:.3rem;top:.3rem;width:30px;height:30px;z-index:1000000;border-radius:30px;background:#ccc url(/weixin/Public/Member/images/mother/music.png) no-repeat;background-size:cover;text-align:center;}
.music img{vertical-align:top;}
.says{width:10rem;margin:0 auto;}
.says a{width:4rem;}
.swiper-container{transform:translate3d(0,100%,0);-webkit-transform:translate3d(0,100%,0);transition:.5s;-webkit-transition:.5s;}
.return{transform:translate3d(0,0,0);-webkit-transform:translate3d(0,0,0);}

@-webkit-keyframes seal{
    20% {
        opacity: 0;
        -webkit-transform: scale(2);
        transform: scale(2)
    }
    100% {
        opacity: 1;
        -webkit-transform: scale(1);
        transform: scale(1)
    }
}

@keyframes seal{
    20% {
        opacity: 0;
        -webkit-transform: scale(2);
        transform: scale(2)
    }
    100% {
        opacity: 1;
        -webkit-transform: scale(1);
        transform: scale(1)
    }
}

.seal{
    -webkit-animation-name: seal;
    animation-name: seal
}
@-webkit-keyframes scales{
    20% {
        -webkit-transform: scale(2);
        transform: scale(2)
    }
    100% {
        -webkit-transform: scale(1);
        transform: scale(1)
    }
}

@keyframes scales{
    20% {
        -webkit-transform: scale(2);
        transform: scale(2)
    }
    100% {
        -webkit-transform: scale(1);
        transform: scale(1)
    }
}

.scales{
    -webkit-animation-name: scales;
    animation-name: scales
}
@-webkit-keyframes slideRight{
    20% {
        -webkit-transform: translate3d(-12rem,0,0);
        transform: translate3d(-12rem,0,0)
    }
    90% {
        -webkit-transform: translate3d(1rem,0,0);
        transform: translate3d(1rem,0,0)
    }
    100% {
        -webkit-transform: translate3d(0,0,0);
        transform: translate3d(0,0,0)
    }
}

@keyframes slideRight{
    20% {
        -webkit-transform: translate3d(-12rem,0,0);
        transform: translate3d(-12rem,0,0)
    }
    90% {
        -webkit-transform: translate3d(1rem,0,0);
        transform: translate3d(1rem,0,0)
    }
    100% {
        -webkit-transform: translate3d(0,0,0);
        transform: translate3d(0,0,0)
    }
}

.slideRight{
    -webkit-animation-name: slideRight;
    animation-name: slideRight;
}
@-webkit-keyframes slideLeft{
    20% {
        -webkit-transform: translate3d(12rem,0,0);
        transform: translate3d(12rem,0,0)
    }
    90% {
        -webkit-transform: translate3d(-1rem,0,0);
        transform: translate3d(-1rem,0,0)
    }
    100% {
        -webkit-transform: translate3d(0,0,0);
        transform: translate3d(0,0,0)
    }
}

@keyframes slideLeft{
    20% {
        -webkit-transform: translate3d(12rem,0,0);
        transform: translate3d(12rem,0,0)
    }
    90% {
        -webkit-transform: translate3d(-1rem,0,0);
        transform: translate3d(-1rem,0,0)
    }
    100% {
        -webkit-transform: translate3d(0,0,0);
        transform: translate3d(0,0,0)
    }
}

.slideLeft{
    -webkit-animation-name: slideLeft;
    animation-name: slideLeft;
}
@-webkit-keyframes rise{
    0% {
        opacity:0;
        -webkit-transform: translate3d(0,100px,0);
        transform: translate3d(0,100px,0)
    }
    40% {
        opacity:1;
        -webkit-transform: translate3d(0,0,0);
        transform: translate3d(0,0,0)
    }
    100% {
        opacity:0;
        -webkit-transform: translate3d(0,-20px,0);
        transform: translate3d(0,-20px,0)
    }
}

@keyframes rise{
    0% {
        opacity:0;
        -webkit-transform: translate3d(0,100px,0);
        transform: translate3d(0,100px,0)
    }
    40% {
        opacity:1;
        -webkit-transform: translate3d(0,0,0);
        transform: translate3d(0,0,0)
    }
    100% {
        opacity:0;
        -webkit-transform: translate3d(0,-20px,0);
        transform: translate3d(0,-20px,0)
    }
}

.rise{
    -webkit-animation-name: rise;
    animation-name: rise;
    -webkit-animation-iteration-count:infinite;
    animation-iteration-count:infinite;
}
@-webkit-keyframes flys{
    0% {
        opacity:0;
        -webkit-transform: translate3d(0,100px,0);
        transform: translate3d(0,100px,0)
    }
    100% {
        opacity:1;
        -webkit-transform: translate3d(0,0,0);
        transform: translate3d(0,0,0)
    }
}

@keyframes flys{
    0% {
        opacity:0;
        -webkit-transform: translate3d(0,100px,0);
        transform: translate3d(0,100px,0)
    }
    100% {
        opacity:1;
        -webkit-transform: translate3d(0,0,0);
        transform: translate3d(0,0,0)
    }
}

.flys{
    -webkit-animation-name: flys;
    animation-name: flys;
}

@-webkit-keyframes move {
    0% {
        opacity: 1
    }
    100% {
        -webkit-transform: translate3d(100%, 100%, 0);
        transform: translate3d(100%, 100%, 0);
        opacity: 0
    }
}

@keyframes move {
    0% {
        opacity: 1
    }
    100% {
        -webkit-transform: translate3d(100%, 100%, 0);
        -ms-transform: translate3d(100%, 100%, 0);
        transform: translate3d(100%, 100%, 0);
        opacity: 0
    }
}

.move {
    -webkit-animation-name: move;
    animation-name: move;
    -webkit-animation-timing-function: ease-in;
    animation-timing-function: ease-in
}

@-webkit-keyframes moveLeft {
    0% {
        opacity: 0;
        -webkit-transform:translate3d(-100px,0,0);
        transform:translate3d(-100px,0,0);
    }
    100% {
        opacity: 1;
        -webkit-transform:translate3d(0,0,0);
        transform:translate3d(0,0,0);
    }
}

@keyframes moveLeft {
    0% {
        opacity: 0;
        -webkit-transform:translate3d(-100px,0,0);
        transform:translate3d(-100px,0,0);
    }
    100% {
        opacity: 1;
        -webkit-transform:translate3d(0,0,0);
        transform:translate3d(0,0,0);
    }
}

.moveLeft {
    -webkit-animation-name: moveLeft;
    animation-name: moveLeft;
    -webkit-animation-timing-function: linear;
    animation-timing-function: linear
}

@-webkit-keyframes appear {
    0% {
        width: 0;
    }
    100% {
        width: 10rem;
    }
}

@keyframes appear {
    0% {
        width: 0;
    }
    100% {
        width: 10rem;
    }
}

.appear {
    -webkit-animation-name: appear;
    animation-name: appear;
    -webkit-animation-timing-function: linear;
    animation-timing-function: linear
}

@-webkit-keyframes rotate {
    0% {
        transform: rotate(0deg);
        -webkit-transform: rotate(0deg);
    }
    50% {
        transform: rotate(180deg);
        -webkit-transform: rotate(180deg);
    }
    100% {
        transform: rotate(360deg);
        -webkit-transform: rotate(360deg);
    }
}

@keyframes rotate {
    0% {
        transform: rotate(0deg);
        -webkit-transform: rotate(0deg);
    }
    50% {
        transform: rotate(180deg);
        -webkit-transform: rotate(180deg);
    }
    100% {
        transform: rotate(360deg);
        -webkit-transform: rotate(360deg);
    }
}

.rotate {
    -webkit-animation:rotate 1s linear infinite;
    animation:rotate 1s linear infinite;
}