/*20150723新页面样式  郑洁  请勿乱删*/
body { margin: 0; padding: 0; background-color: #e5e5e5; font-family: "微软雅黑"; }
ul, li, h1, h2, h3, h4, sapn, dl, dd, dr, dt, i, em { list-style: none; margin: 0; padding: 0; font-style: normal; text-decoration: none; }
a { text-decoration: none; }
a:hover{ text-decoration: none; }
.fl{float: left;}
.fr{float: right;}
.clearfix:after{content: ""; display: block; clear: both;}
.left{ float:left;}
.right{ float:right;}
.relative {  position: relative;  left: 0;  top: 0;}
.wei_web img { width: 100% }
.wei_web .booking_button { margin: 15px }
.index_list { list-style: none outside none; margin: 0 }
.index_list li { display: inline-block; float: left; width: 33.3% }
.index_list li a { cursor: pointer; display: inline-block; width: 100%; padding-bottom: 13px; border: 1px solid #e2e1e1; background-color: #fff }
.index_list_img p { padding-top: 10px; text-align: center; color: #000; font-size: 11px }
.index_list_img p img { width: 100%; margin-top: 10px }
.ui-content { padding: 0 }
/*头部*/
.hos_deil{ background:url(/weixin/Application/Member/View/images/hos/headbg.jpg) no-repeat; height:150px; width:100%; background-size:cover;}
.hos_deil h2{ font-size:18px; color:#fff; font-weight:normal; text-align:center; padding-top:40px; line-height:50px;}
.hos_deil span{font-size:14px; color:#fff; float:left; line-height:40px; margin-right:15px;}
.hos_deil span b{ font-weight:normal;}
.hos_deil .collect{ background:#ec9800 url(/weixin/Application/Member/View/images/hos/shoucang.png) no-repeat 5px 13px; text-align:center; float:left; width:80px; height:45px; border-top-left-radius:3px; border-bottom-left-radius:3px; color:#fff;font-size:14px; line-height:45px; padding-left:5px;}
.hos_deil .al_collect{ background:#fff url(/weixin/Application/Member/View/images/hos/yisc.png) no-repeat 5px 13px; text-align:center; float:left; width:60px; height:45px; border-top-left-radius:3px; border-bottom-left-radius:3px; color:#ec9800;font-size:14px; line-height:45px; padding-left:20px;}


.head ul{ background:#fff; height:85px; margin-bottom: 10px; border-bottom:2px solid #f2f0f0;}
.head ul li{ float:left; width:20%; margin-top:10px; height:77px;}
.head ul li a{ display:block; text-align:center; font-size:20px; color:#636871; margin:0 auto; height:75px; width:90%;}
.head ul li a .mat_10{ margin-top:10px;}
.head ul li a .font14{ font-size:12px;}
.head ul li a img{ width:40px; height:40px;}
.head ul li a.curr{ height: 72px; border-bottom:2px solid #ff647c;}
.head ul li a.curr p{ color:#ff647c;}

/*开通服务栏目头部以下*/
.open_service{ background:#fff;}
.open_service ul{ padding:15px; }
.open_service ul li{ border-bottom:1px dashed #b6b6b6;overflow:hidden; margin-bottom:12px;}
.open_service .ser_left{ width:30%;}
.open_service .ser_left img{ width:95px; height:auto; margin-top:5px;}
.open_service .ser_left p{ font-size:14px; color:#333333; text-align:center; line-height:30px;}
.open_service .ser_right{ width:65%;}
.open_service .ser_right p{ line-height:22px; height:90px; overflow:hidden; margin-top:3px; min-height:90px;}
.open_service .ser_right .jiage{ margin:15px 0; height:30px;}
.open_service .ser_right .jiage b{ color:#f84b23; font-size:12px; font-weight:normal; line-height:30px;}
.open_service .ser_right .jiage button{ background:#ff6600; width:100px; height:30px; line-height:28px; color:#fff; font-size:14px; border:none; border-radius:5px; float:right;}

/*医院详情栏目*/
.hos_jianjie{ background:#fff; padding:15px; overflow:hidden;}
.jianjie_top{ float:left; width:100%;}
.jianjie_top h2{font-size:18px; color:#333333; font-weight:normal;  line-height:30px; float:left; width:70%;}
.jianjie_top span{ float:right; font-size:16px; color:#f9840f;line-height:25px;}
.jianjie_bot{ float:left; margin-top:15px; width: 100%; margin-bottom: 15px;}
.jianjie_bot img{  width:32%; height:auto; float:left; margin-top:5px;}
.jianjie_bot ul{ float:right; width:64%; font-size:14px; line-height:24px; color:#999999;}
.jianjie_bot ul li div{width: 48%; overflow: hidden;}
.jianjie_bot ul li b{ color:#333333; font-weight:normal;}
.jianjie p{line-height:24px; color:#333; font-size:14px; padding-top:10px; margin-bottom:10px; float:left;}

/*交通线路栏目*/
.jiaotong{ background:#fff; padding:15px;}
.jiaotong img{ width:100%; height:auto;}
.dizhi{height:100px; margin-top:15px; border-bottom:1px dashed #b6b6b6;}
.dizhi h2{font-size:18px; color:#333333; font-weight:normal;  line-height:30px; margin-bottom:10px;}
.dizhi p{ color:#919191; font-size:14px; display:block; background:url(/weixin/Application/Member/View/images/hos/dizhi.png) no-repeat left center; height:30px; line-height:30px; padding-left:30px; background-size:25px; float:left;}
.dizhi a{ display:block; background:#ff6600; text-align:center; line-height:30px; width:100px; float:right; color:#fff; border-radius:5px;}

/*医院荣誉栏目*/
.rongyu{ background:#fff; padding:15px; overflow:hidden;}
.rongyu h2{font-size:18px; color:#333333; font-weight:normal;  line-height:30px; float:left; margin-bottom:10px;}
.rongyu .ry_con{ float:left; width:100%;}
.rongyu .ry_con li{ background:url(/weixin/Application/Member/View/images/hos/libg.png) no-repeat 10px 0px; padding-left:40px; line-height:25px; margin:10px 0;}
.rongyu .ry_con li span{ color:#323232; font-size:16px;}
.rongyu .ry_con li b{ color:#919191; font-size:12px; font-weight:normal;}








