* {
	margin: 0;
	padding: 0;
	font-family: 'microsoft yahei';
}
img{vertical-align: middle;}
a {
  text-decoration:none;
  border:none;
  -webkit-tap-highlight-color: rgba(0,0,0,0);
  -webkit-tap-highlight-color:transparent;
  outline:none;
}
html,body{
	width: 100%;
	height: 100%;
	background: #712bc0;
}
@keyframes circle {
  from { transform:rotate(0deg); }
  to { transform:rotate(360deg); }
}
@keyframes inner-circle{
  from{
    transform: rotate(360deg);
  }
}
#loutinav {
	width: 100%;
	height: 1.6rem;
	text-align: center;
	position: fixed;
	top: 0;
	left: 0;
	background: #9354fd;
	display: none;
	z-index: 2;
}

#loutinav ul{
	overflow: hidden;
}

#loutinav ul li{
	width: 3.75rem;
	height: 100%;
	line-height: 1.6rem;
	float: left;
	text-align: center;
	list-style: none;
	font-size: 0.52rem;
	color: #fff;
}

#loutinav ul li.active{
	background: #e38521;
	display: block;
	border-radius: .5rem .5rem 0 0;
}
#top{
	position: fixed;
	right: .3rem;
	bottom: 2.5rem;
	z-index: 2;
	display: none;
}
#top img{
	width: 2.08rem;
}

.louti,.f6{
	width: 14.2rem;
	margin: .2rem  auto;
	overflow: hidden
}
#main .louti a,#main .f6 a{
	display: block;
	float: left;
	position: relative;
	z-index: 1;
}
.louti1 a,.f6 a{
	width: 6.98rem;
	margin: .2rem .2rem 0 0;
}
#main .louti1 a:nth-child(2n){
	margin-right:0;
}
.louti2 a{
	width: 4.62rem;
	margin: .15rem .15rem 0 0;
}
.louti3 a,.louti4 a,.louti5 a,.louti6 a{
	width: 7rem;
	margin: .15rem .15rem 0 0;
}
.louti3 a:nth-child(2n-1),.louti4 a:nth-child(2n-1),.louti5 a:nth-child(2n-1),.louti6 a:nth-child(2n-1){
	margin-right:0;
}
.louti .f-title img,.f6 .f-title img{
	display: block;
	width: 6.04rem;
	margin: .62rem auto .8rem;
}
.louti.louti0{
	width: 100%;
	height: 17.1rem;
	background: url("../images/zhc/zhc-banner.jpg");
	background-size: 100% 100%;
	margin: 0 auto;
}
.louti1 img{
	width: 6.98rem;
}
.louti2 img{
	width: 4.62rem;
}
.louti3 img ,.louti4 img,.louti5 img,.louti6 img{
	width: 7rem;
}
.louti1 i{
	width: 2rem;
	height: .48rem;
	background: url("../images/zhc/mysy01.png");
	background-size: 100% 100%;
	position: absolute;
	top: 0;
	right: 0;
}
.louti2 i{
	width: 1.4rem;
	height: 1.4rem;
	background: url("../images/zhc/mysy02.png");
	background-size: 100% 100%;
	position: absolute;
	top: 0;
	right: 0;
}
.louti4 i{
	width: 1.84rem;
	height: 1.84rem;
	background: url("../images/zhc/mysy03.png");
	background-size: 100% 100%;
	position: absolute;
	top: 0;
	right: 0;
}
.louti6 i{
	width: 1.34rem;
	height: 0.98rem;
	background: url("../images/zhc/rm.png");
	background-size: 100% 100%;
	position: absolute;
	top: 0;
	left: 0;
}
