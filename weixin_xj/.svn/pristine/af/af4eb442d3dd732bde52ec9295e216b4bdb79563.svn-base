/*common css*/
*{padding: 0; margin: 0;}
img{vertical-align: top;}
li{list-style: none;}
a{text-decoration: none;-webkit-tap-highlight-color:rgba(0,0,0,0);}
i{font-style: normal;}
.fl{float: left;}
.fr{float: right;}
.clear:after{display: block; content: ""; clear: both;}
/*main*/
.tit{width: 100%;}
.tit img{width: 100%;}
.list{padding: 5px 3.2% 0;}
.list li{padding: 8px 0; border-bottom: 1px solid #e7e7e7;}
.list li a{display: block; position: relative; z-index: 2; padding-right: 20px; background: url(/weixin/Public/Member/images/user/jiantou1.png) no-repeat right center; background-size: 8px;}
.list .date{padding: 5px 0;}
.list .date p{font-size: 14px; height: 25px;line-height: 25px; color: #333;}
.list .date span{font-size: 12px; color: #999; display: block; height: 25px;line-height: 25px;}
.list li .process{width: 36px; height: 60px;background: url(/weixin/Public/Member/images/user/process2.jpg) no-repeat center; background-size: 16px; }
.list li:last-of-type .process{background: url(/weixin/Public/Member/images/user/process3.jpg) no-repeat center bottom; background-size: 16px; position: relative; }
.list li:first-of-type .process{background: url(/weixin/Public/Member/images/user/process1.jpg) no-repeat center bottom; background-size: 16px; position: relative; }
.list .con{padding: 5px 0;}
.list .con .hos{height: 25px;line-height: 25px; color: #333; font-size: 14px;}
.list .con .des{height: 25px; width: 100px; white-space:nowrap; overflow:hidden; text-overflow:ellipsis; line-height: 25px; color: #999; font-size: 12px;}
.list .img-wrap{position: relative; z-index: 4; margin-left: 4px;}
.list .img-wrap img{ width: 60px; height: 60px;}
.list .img-wrap span{position: absolute; right: 0px; bottom: 0px; width: 20px; height: 20px; background: url(/weixin/Public/Member/images/user/fangda.png) no-repeat; background-size: 20px; z-index: 3;}
.list .img-wrap .mask{position: absolute; left: 0; top: 0; width: 60px; height: 60px; background: rgba(0,0,0,0.5); z-index: 2;}
.chakan{display: none; position: absolute; left: 0; top: 0; width: 100%; height: 100%; z-index: 10; text-align: center; background: rgba(0,0,0,0.7);}
.chakan span{position: absolute; left: 50%; bottom: 60px; margin-left: -3em; color: #fff; background: #ff647c; width: 6em; text-align: center; height: 2em; line-height: 2em; border-radius: 6px;  background-size: 25px;}
.chakan img{width: 90%; margin-top: 40px;}
.jianli{position: fixed; z-index: 50; left: 0; bottom: 0; height: 40px; width: 100%; display: block; line-height: 40px; text-align: center; background: #ff647c; color: #fff; font-size: 14px;}
.pswp__button {
    position: fixed;
    right: 50%;
    bottom: 15px;
    width: 6em;
    height: 2em;
    color: #fff;
    font-size: 18px;
    margin-right: -3em;
    background-color: #ff647c;
    background-image: none;
    z-index: 1100;
    line-height: 2em;
    border-radius: 6px;
}