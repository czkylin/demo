<script>
var over_footer = '0';
function search_list(level_id,province_id,hos_name)
{
	if (level_id != '')
	{
		$('#current_level').text(level_id);
	}
	if (province_id != '')
	{
		$('#current_province').text(province_id);
	}


	$('#current_name').text(encodeURI(hos_name));

	if ($('#current_level').text() == '0')
	{
		$('#current_level').text('');
	}
	if ($('#current_province').text() == '0')
	{
		$('#current_province').text('');
	}


	$('#current_pagenum').text('1');
	$('#hos-list').html('');
	$('.ui-jiazai').children('a').text('加载更多');

	loadmore('#current_pagenum','/weixin/index.php?m=Member&c=Hos&a=hos_list_append&hos_name='+$('#current_name').text()+'&level_id='+$('#current_level').text()+'&province_id='+$('#current_province').text()+'','#hos-list');
}

function load_more()
{
	loadmore('#current_pagenum','/weixin/index.php?m=Member&c=Hos&a=hos_list_append&hos_name='+$('#current_name').text()+'&level_id='+$('#current_level').text()+'&province_id='+$('#current_province').text()+'','#hos-list');
}
</script>
