.footer{position:fixed;bottom:0;left:0;width:100%}.footer nav{border-top:1px solid #ccc;background:#fff}.footer ul{display:box;display:-webkit-box}.footer li{width:25%;display:box;display:-webkit-box;-webkit-box-pack:center;-webkit-box-align:center}.footer li a{display:block;width:100%;height:.4rem;color:#333;position:relative;padding:.05rem 0}.footer li i,.footer li span{display:block;width:100%;text-align:center}.footer i.ih{margin-left:0}.footer li i{height:1.07rem}.footer li span{font-size:14px;height:1.07rem;line-height:1.49rem}.footer li.active i,.footer li.active span{color:#64bbff}