body{
	background: #fff;
}
.clearfix:after{
	content: "";
	display: block;
	clear: both;
}
/*notice*/
@keyframes show{
	100%{
		height: 1.87rem;
		opacity: 1;
	}
}
@-webkit-keyframes show{
	100%{
		height: 1.87rem;
		opacity: 1;
	}
}
.notice{
	height: 0rem;
	width: 100%;
	background: #fffdeb;
	margin-bottom: 0.2rem;
	opacity: 0;
	animation:show 0.8s 0.5s linear forwards;
	-webkit-animation:show 0.8s 0.5s linear forwards;
}
.notice a{
	display: block;
	width: 95%;
	margin: 0 auto;
	height: 1.87rem;
	background: url(/weixin/Public/Member/images/jiating/clock.jpg) no-repeat left center;
	background-size: 18px;
}
.notice span{
	font-size: 0.6rem;
	line-height: 1.87rem;
	margin-left: 1.28rem;
	color: #565758;
}
.notice i{
	font-style: normal;
	font-size: 12px;
	color: #333;
	line-height: 1.87rem;
	float: right;
}
/*main*/
.list{}
.list li{
	padding: .5rem 8%;
	border-bottom: 1px solid #dbdbdb;
}
.list li .touxiang{
	width: 2.2rem;
	height: 2.2rem;
	border-radius: 2.2rem;
	float: left;
	margin-top: 0.25rem;
	margin-right: 0.32rem;
}
.list .mes{
	float: left;
}
.list .mes p.type{
	font-size: 0.68rem;
	color: #333;
	font-weight: bold;
	line-height: 1rem;
}
.list .mes p.name{
	font-size: 14px;
	color: #333;
	line-height: 1rem;
}
.list .mes p.add{
	font-size: 14px;
	color: #999999;
	line-height: 1rem;
}
.con{
	width: 60%;
	margin: 0.85rem auto;
}
.con .con-wrap{
	width: 2.5rem;
	text-align: center;
}
.con .con-wrap a{
	color: #000;
}
.con .con-wrap img{
	
}
.con .con-wrap p{
	font-size: 0.55rem;
}
