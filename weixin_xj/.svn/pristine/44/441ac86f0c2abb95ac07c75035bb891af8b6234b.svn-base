.clearfix:after{content: '';display: block; clear: both;}
.user-title{padding: 10px 24px; background: url(/weixin/Public/Member/images/gerenzhongxin/header-bg.jpg) no-repeat; background-size: 100% 100%; position: relative; overflow: hidden;}
.user-title .touxiang{width: 1.172rem; height: 1.172rem; position: relative; z-index: 5; text-align: center; margin: 0 auto; background: url(/weixin/Public/Member/images/gerenzhongxin/touxiang-quan.png) no-repeat center; background-size: 1.172rem 1.172rem;}
.user-title .touxiang img{width: 0.616rem; height: 0.616rem; border-radius: 0.616rem; border: 3px solid #fff; margin-top: 0.28rem;}
.user-title .touxiang .name{position: absolute; bottom: 5px; left: 0; display: block; width: 100%; text-align: center; font-size: 16px; color: #fff;}
.user-title .touxiang .name span{width: 0.1rem; height: 0.1rem; display: inline-block; background:no-repeat center; background-size: 0.1rem;}
.user-title .touxiang .name span.nan{background-image: url(/weixin/Public/Member/images/gerenzhongxin/nan.png);}
.user-title .touxiang .name span.nv{background-image: url(/weixin/Public/Member/images/gerenzhongxin/nv.png);}
.user-title .tel{ position: relative; z-index: 5; width: 100%; height: 0.18rem; line-height: 0.18rem; text-align: center; font-size: 14px; color: #fff;}
.user-title .reg-time{ position: relative; z-index: 5; width: 100%; height: 0.18rem; line-height: 0.18rem; text-align: center; font-size: 14px; color: #fff;}
.user-title .mask{display: none; position: absolute; left: 0; top: 0; bottom: 0; right: 0; z-index: 1; width: 100%; height: 100%;}


.hot{border-bottom: 10px solid #eeeeee;}
.hot .hot-title{display:block; border-bottom: 1px solid #e0e0e0; padding:14px 24px; background:#fff; border-radius:0;color: #333; overflow:hidden;position:relative; padding-bottom:0;}
.hot .hot-title span{float:left; width:30px;}
.hot .hot-title span img{width:20px; height:20px;}
.hot .hot-title b{font-size: 14px; color: #333; font-weight:normal; display:block; margin-left:30px; padding-bottom:14px;}
.hot .hot-title i{position:absolute; top:15px; right:25px;}
.hot .hot-title i img{width:8px;}
.hot-list{padding: 10px 0;}
.hot-list li,.hot-list li a{float: left; display: block; text-align: center;}
.hot-list li a{width: 100%;}
.hot-list li{width: 20%;}
.hot-list li img{width: 36%;}
.hot-list li p{color: #999999; font-size: 12px; line-height: 20px; text-align: center;}

