.info1 b{padding-bottom:0;}
.info1 i,.info2 i{ position:absolute; top:15px; right:24px;}
.info1 i img,.info2 i img{width: 8px;}
.info2 b span{ float:none;}
.info1 a:last-child b{ border:0;}
.info2 a:last-child b{ border:0;}



.hezuoHos { padding: 20px 12px 20px 120px; height: auto;  position: relative; background-color: #f5f5f5; -webkit-box-sizing: border-box; box-sizing: border-box; height:105px;}
.hezuoHos .yiyuanpic { position: absolute; left: 12px; top:20px; width: 102px; height: 64px; max-height: 64px;border-radius: 5px; overflow:hidden; }
.hezuoHos .yiyuanpic img{width: 100%; border-radius: 5px; }
.hezuoHos .yiyuanpic b{width: 20px; height: 20px; background: url(/weixin/Public/Member/images/icon/new.png) no-repeat; background-size: 20px; position: absolute; left: 0; top: 0;}
.hezuoHos p { line-height: 24px; font-size: 14px; text-overflow: ellipsis; white-space: nowrap; overflow: hidden; color: #666; }
.hezuoHos .hezuoHosdes p{ margin: 0 2px; }
.hezuoHos p b { font-weight: normal; float: right; }
.hezuoHos .juli{font-size: 12px; position: absolute; top: 0px; right: 8px; color: #666; padding-left: 10px; background: url(/weixin/Public/Member/images/icon/address.png) no-repeat 1px center; background-size: 8px;}
.hezuoHosdes{height: 20px; overflow: hidden;}
.hezuoHosdes p{float: left;}




