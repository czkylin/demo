.info1,.info2{border-bottom: 10px solid #eeeeee; padding: 0 0 0 24px; }
.info1{padding:0;}
.info1 a,.info2 a{ display:block; padding:14px 0; background:#fff; border-radius:0;color: #333; overflow:hidden;position:relative; padding-bottom:0; border-bottom: 1px solid #e0e0e0;}
.info1 a{padding:14px 0 14px 24px;}
.info1 a:last-child,.info2 a:last-child{border-bottom: none;}
.info1 span,.info2 span{ float:left; width:30px;}
.info1 span img,.info2 span img{ width:20px; height:20px;}
