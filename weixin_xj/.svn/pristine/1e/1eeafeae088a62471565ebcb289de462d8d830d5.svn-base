body{background:#fff;color:#333;font-size:16px;}
.ui-icon-unfold{color:#333;}
.down{transform:rotateX(180deg);-webkit-transform:rotateX(180deg);}
.head li{width:5rem;text-align:center;padding:5px 0;border-bottom:1px solid #8c8c8c;}
.head li.active{border-bottom:1px solid #4891ef;}
.head li.active a{color:#4891ef;}
.head li:nth-child(1) a{border-right:1px solid #666;}
.head a{color:#666;display:block;width:100%;height:30px;line-height:30px;}
.pics{background:#f0ede9 url(/weixin/Public/Expert/images/teams/bg.jpg) no-repeat;background-size:100%;}
.unit{position:relative;}
.unit a{position:absolute;top:0;left:0;width:100%;height:100%;}
.team{padding:.3rem;border-bottom:1px solid #8c8c8c;}
.team h2{font-weight:normal;font-size:16px;padding-bottom:10px;}
.team p{color:#8c8c8c;font-size:14px;}
.team .left{width:8rem;}
.team .right{width:1.4rem;}
.st{padding:1rem .3rem;background:#f3f3f3;}
.st .left{width:8rem;}
.st .right{width:1.4rem;padding:20px 0;}
.st i{font-style:normal;color:#666;}
.st i.wait,span.wait{color:#ff8500;}
span.sta{font-size:14px;}
span.no{color:#e4583f;}
.img{border:1px solid #e2e2e2;width:60px;height:60px;border-radius:60px;margin-right:10px;}
.txt{padding:5px 0;}
.txt h3{font-weight:normal;margin-bottom:5px;}
.txt em{font-style:normal;color:#666;}
.left .text{width:6rem;}
.text{width:7.4rem;}
.text small{font-size:12px;margin:0 10px;}
.text em{font-style:normal;color:#ff8500;border:1px solid #ff8500;border-radius:4px;display:inline-block;padding:5px;vertical-align:bottom;font-size:14px;}
.text h3{font-size:16px;font-weight:normal;padding-bottom:10px;}
.image{width:1.5rem;height:1.5rem;padding:0.25rem 0.5rem 0.25rem 0;}
.tm{margin:.3rem;border-bottom:1px solid #8c8c8c;}
.noLine .tm,.noLine .team{border-bottom-width:0;}
.tm h2{font-weight:normal;font-size:20px;padding-bottom:16px;}
.tm p{color:#8c8c8c;padding-bottom:16px;line-height:24px;}
.tm span{margin-right:20px;}
.tm strong{font-weight:normal;color:#333;}
.tm b{font-weight:normal;color:#e4583f;}
.btn-group a:nth-child(1){margin:1rem 0.5rem 1rem 1.5rem;}
.btn-group a:nth-child(2){margin:1rem 1.5rem 1rem 0.5rem;}
.btn{border-radius:6px;border:1px solid #4891ef;color:#4891ef;display:block;height:30px;line-height:30px;width:3rem;text-align:center;}
.btn-blue{background:#4891ef;color:#fff;border:1px solid #4891ef;}
.mask{background:rgba(0,0,0,0.6);position:fixed;top:0;left:0;width:100%;height:100%;transition:.3s;-webkit-transition:.3s;}
.opacity{visibility:hidden;opacity:0;}
.option{position:fixed;bottom:0;left:0;width:100%;background:#fff;transition:.3s;-webkit-transition:.3s;}
.toDown{visibility:hidden;opacity:0;transform:translate3d(0,100px,0);-webkit-transform:translate3d(0,100px,0);}
.oList{padding:20px;}
.oList label{display:block;padding-top:10px;}
.oList h3{font-weight:normal;}
.oList textarea{width:90%;padding:4%;margin-top:10px;}
.oList input{display:none;}
.oList span{width:16px;height:16px;border-radius:16px;border:1px solid #333;display:inline-block;vertical-align:middle;margin-top:-4px;margin-right:6px;position:relative;}
.oList input:checked~span{border-color:#ff8500;}
.oList input:checked~span:before{position:absolute;top:55%;left:4px;width:4px;height:1px;content:'';background:#ff8500;transform:rotateZ(45deg);-webkit-transform:rotateZ(45deg);}
.oList input:checked~span:after{position:absolute;top:50%;left:6px;width:8px;height:1px;content:'';background:#ff8500;transform:rotateZ(-45deg);-webkit-transform:rotateZ(-45deg);}
.over{margin:30px auto 0;}
.head p{color:#666;}
.limit{overflow:hidden;text-overflow:ellipsis;-webkit-box-orient:vertical;-webkit-line-clamp:2;display:-webkit-box;}
.tc{border-top:1px solid #d5d3d0;padding:.3rem;}
.tc p{font-size:14px;color:#666;}
.last{padding:.3rem;color:#666;}
.join{color:#fff;background:#4891ef;position:fixed;bottom:0;left:0;width:100%;text-align:center;height:40px;line-height:40px;}