header, section, footer, aside, article, figure { display: block; }

/*当屏幕宽度小于1540时读取的样式*/
@media screen and (max-width:1540px) {
}
.headPaiHang { width: 100%; }
.headPaiHang .top { width: 100%; height: 60px; line-height: 60px; background: #27262b; font-size: 20px; color: #fff; }
.headPaiHang .top a { color: #fff; }
.headPaiHang .top a.fl { margin-left: 3%; }
.headPaiHang .top a.fl img { margin-bottom: -8px; margin-right: 10px; }
.headPaiHang .top div.fr { margin-top: 15px; margin-right: 3%; }
.headPaiHang .top div.fr>a { display: block; }
.headPaiHang .topsec { width: 100%; height: 40px; border-bottom: 1px solid #b5b5b5; }
.headPaiHang .topsec a { display: inline-block; margin-top: 10px; margin-left: 3%; }
.headPaiHang .bottom { width: 100%; height: 140px; background: url(../../images/fenXiang/pic2.jpg) no-repeat; background-size: cover; }
.headPaiHang .bottom2 { background: url(../../images/fenXiang/pic4.jpg) no-repeat; }
.headPaiHang .bottom3 { width: 100%; height: auto; }
.headPaiHang .bottom3 img { width: 100%; }
.headPaiHang .bottom dl.pa { width: 200px; right: 3%; bottom: -28px; }
.headPaiHang .bottom dl.pa dt { width: 100px; float: right; text-align: center; }
.headPaiHang .bottom dl.pa dd { width: 100px; line-height: 70px; float: right; }
.headPaiHang .bottom dl.pa a { display: block; color: #fff; }
.headPaiHang .bottom dl.pa dt a.fs20 { display: block; height: 30px; line-height: 30px; padding-top: 10px; }
.headPaiHang .bottom dl.pa dt a.fs16 img { margin-bottom: -6px; }
.headPaiHang .bottom dl.pa dt a.fs16 { width: 120px; padding-top: 15px; color: #3C3C3C; }
.headPaiHang .bottom dl.pa dd img { width: 80px; height: 80px; border: 2px solid #fff; }
.content { padding: 0 3%; }
/*排行榜*/
.Tab_CaiDan { width: 100%; height: 40px; }
.Tab_CaiDan a { width: 50%; display: block; border: 1px solid #ff647c; -webkit-box-sizing: border-box; box-sizing: border-box; float: left; height: 38px; line-height: 38px; text-align: center; color: #333333; font-size: 14px; }
.Tab_CaiDan a:last-child { border-left: 0; }
.Tab_CaiDan .curr { background: #ff647c; color: #fff; }
.hot li { position: relative; width: 100%; border-bottom: 1px solid #eee; padding: 10px 0; }
.hot .ww { width: 35px; left: 0px; top: 18px; text-align: center; color: #999; font-size: 22px; }
.hot .ww img { display: block; width: 25px; height: 25px; margin: 0 auto; }
.hot .touxiang { width: 35px; height: 35px; left: 40px; top: 10px; }
.hot .touxiang a img { width: 40px; height: 40px; border-radius:50%; }
.hot .huoyuedu { width: 100%; height: 40px; padding-left: 85px; padding-right: 10px; -webkit-box-sizing: border-box; box-sizing: border-box; }
.hot .huoyuedu .title { width: 100%; height: 22px; font-size: 16px; color: #333; }
.hot .huoyuedu .title span, .hot .huoyuedu .title a { display: inline-block; }
.hot .huoyuedu .title span{ font-size: 14px; height: 22px; line-height: 22px; }
.hot .huoyuedu .title span.fr{ color:#999; font-size:12px;}
.hot .huoyuedu .title a { margin-left: 10px; font-size: 12px; height: 22px; line-height:22px; color:#999; width:50%;}
.hot .huoyuedu div.pr { width: 100%; height: 8px; background: #dbdbdb; margin-top: 10px; border-radius: 6px; }
.hot .huoyuedu div.pr .pa { left: 0; height: 8px; top: 0; z-index: 3; background: #ffde5d; border-radius: 6px; }
.gslmLabel .labelCon { width: 91%; height: 43px; float: left; overflow-x: scroll; overflow-y: hidden; }
.gslmLabel .labelCon a { height: 30px; line-height: 30px; float: left; margin-right: 10px; margin-top: 5px; color: #333; padding: 0 5px; }
.gslmLabel .labelCon a.curr { background: #ff647c; color: #fff; }
/*医生列表*/
.zhuanJia_list { width: 100%; }
.zhuanJia_list li { position: relative; border-bottom: 1px solid #cacdcc; padding: 15px 0; }
.zhuanJia_list li a.pic { display: block; position: absolute; width: 70px; height: 50px; }
.zhuanJia_list li a img { width: 70px; height: 78px; }
.zhuanJia_list li:last-child { border-bottom: none; }
.zhuanJia_list li .lm_right { padding-left: 80px; width: 100%; position: relative; -webkit-box-sizing: border-box; box-sizing: border-box; }
.zhuanJia_list li .lm_right .new_time{ position: absolute; right: 0; top: 0; font-size: 12px; color:#999; line-height: 16px; }
.zhuanJia_list li .lm_right .mingcheng { float: left; font-size: 14px; color: #000; font-weight: 400; height: 20px; overflow: hidden;display:block; }
.zhuanJia_list li .lm_right i { clear: both; color: #999; float: left; font-size: 12px;display:block; }
.zhuanJia_list li .lm_right span {clear: both; color: #707070; font-size: 12px; height: 25px; line-height: 25px; overflow: hidden; display:block;}
.zhuanJia_list li .lm_right p{ width: 100%; text-overflow: ellipsis; overflow: hidden; white-space: nowrap; height:17px; line-height: 20px;}
