@charset "utf-8";
a, input, button { -webkit-tap-highlight-color: rgba(0,0,0,0); }
/* CSS Document */

html, body {position: relative; height: 100%; overflow:hidden;}
body { background: #fff; font-family: '微软雅黑'; font-size: 14px; color: #000; margin: 0; padding: 0; }
p { padding: 0; margin: 0; }
.clear:after { display: block; content: ''; clear: both; }
.f_l { float: left; }
.f_r { float: right; }
.swiper-container { width: 100%; height: 100%; }
.swiper-slide { font-size: 18px; background: #fff; width: 100%; box-sizing: border-box; }
.page1 { background: url(/weixin/Public/Member/images/daJiangTang/jiangtang/page1_bg.jpg) no-repeat; width: 100%; background-size: 100% 100%; position:relative; }
.yun1 { height: 100%; width: 100%; background:url(/weixin/Public/Member/images/daJiangTang/jiangtang/yun1.png) no-repeat; -webkit-animation: cloud 20s linear infinite; -o-animation: cloud 40s linear infinite; }
@-webkit-keyframes cloud {
from {
background-position:0% 0%
}
to {
background-position:50% 50%
}
}
@-webkit-keyframes move{
	0%{
		-webkit-transform:translateY(10px);
		transform:translateY(10px);
	}
	50%{
		-webkit-transform:translateY(0px);
		transform:translateY(0px);
	}
	100%{
		-webkit-transform:translateY(10px);
		transform:translateY(10px);
	}
}
@keyframes move{
	0%{
		-webkit-transform:translateY(10px);
		transform:translateY(10px);
	}
	50%{
		-webkit-transform:translateY(0px);
		transform:translateY(0px);
	}
	100%{
		-webkit-transform:translateY(10px);
		transform:translateY(10px);
	}
}
.arrow{
	-webkit-animation:move 1s linear infinite;
	position: absolute; height: 0.5rem; display: block; width: 100%; left: 0; bottom: 20px; background: url(/weixin/Public/Member/images/daJiangTang/jiangtang/arrow.png) no-repeat center; background-size: 0.54rem 0.5rem;
}
.page1_con{ position:absolute; top:29%; left:10%; width:100%;}
.page1_con img{ width:80%;}
.page2 { background: url(/weixin/Public/Member/images/daJiangTang/jiangtang/page2_bg.jpg) no-repeat; width: 100%; background-size: 100% 100%;}
.page3 { background: url(/weixin/Public/Member/images/daJiangTang/jiangtang/page3_bg.jpg) no-repeat; width: 100%;  background-size: 100% 100%;}

.page3_con{ position:absolute; top:5%;  width:100%;}
.page3_con img{ width:100%;}
.img_con{ position:absolute; bottom:0;  width:100%;}
.img{ position:relative; margin-top:50%; padding:15px; height:50%;}
.img img{ width:55%;}
.img img.img1{ bottom:20px; left:0; position:absolute;}
.img img.img2{ bottom:20px; right:0; position:absolute;}
.img img.img3{ bottom:80px; left:25%; position:absolute;}
.img img.img4{ bottom:0px; left:32%; position:absolute;width:40%;}

.page4{ background: url(/weixin/Public/Member/images/daJiangTang/jiangtang/page4_bg.jpg) no-repeat; width: 100%;  background-size: 100% 100%;}
.page5{ background: url(/weixin/Public/Member/images/daJiangTang/jiangtang/page5_bg.jpg) no-repeat; width: 100%;  background-size: 100% 100%;}
.page5_con{ position:absolute; top:35%; left:15%; width:100%;}
.page5_con img{ width:70%;}
.page5 a{ display:block; text-align:center; position:absolute; top:57%; width:100%;}
.page5 img.more{ width:28%;}




