body { position: relative; }
.sear { width: 50%; height: 40px; line-height: 40px; text-align: center; float: left; color: #ff647c; background: #fff; font-size: 14px; }
.sear:nth-child(2) { float: right; }
.sear.active { background: #ff647c; }
.sear.active a { color: #fff; }
.sear a { color: #ff647c; display: block; width: 100%; height: 100%; font-weight: 800; }