body{background:#fff;}
img{vertical-align:top;}
#line,#line .swiper-wrapper,#line .swiper-slide{height:100%;}
#line .swiper-wrapper{transition:.5s;-webkit-transition:.5s;}
#line .swiper-slide.sActive{transform:scale3d(1,1,1);-webkit-transform:scale3d(1,1,1);}
#line .swiper-slide{background:#fff;display:box;display:-webkit-box;-webkit-box-pack:center;-webkit-box-align:center;transform:scale3d(0.8,0.8,0.8);-webkit-transform:scale3d(0.8,0.8,0.8);transition:.5s;-webkit-transition:.5s;}
#line .unit{background:#fff;width:100%;height:100%;border-radius:8px;}
.sTop{border-radius:8px 8px 0 0;overflow:hidden;height:2rem;padding-top:10%;}
.sTop img{width:100%;vertical-align:top;}
.sTop p{text-indent:2em;margin-top:18px;font-size:14px;height:20px;line-height:20px;}
.sBottom{border-radius:0 0 8px 8px;height:50%;background:#fff;}
.sBottom h2{font-size:18px;background:#fff;height:60px;line-height:60px;margin-bottom:.8rem;font-weight:normal;}
.sBottom span{height:80px;line-height:80px;text-align:center;position:relative;}
.sBottom span:before{position:absolute;bottom:-30px;left:0;border-bottom:1px solid #ccc;content:'';width:100%;}
.sBottom p{color:#666;font-size:16px;}
.sBottom img{height:100%;}
.text-center{text-align:center;}
header{border-width:0px;}
#line{position:fixed;top:0;left:0;width:100%;height:100%;z-index:9999;background:#fff;}
.spa{width:100%;height:.5rem;position:fixed;left:0;bottom:0;text-align:center;z-index:99999;margin:0 0.1rem;margin:0;}
.spa span{width:.1rem;height:.1rem;display:inline-block;vertical-align:top;margin:0 0.1rem;border:1px solid #ff647c;border-radius:.1rem;background:#fff;}
.spa span.swiper-pagination-bullet-active{background:#ff647c;}
#test{position:absolute;bottom:0.6rem;left:50%;width:90%;margin-left:-1.35rem;}
#test strong{float: left;}
#test label{float: right;}
#test label input{display: none;}
#test label span{display: block;}
#test strong,#test label span{height: 0.28rem; line-height: 0.28rem; width: 1.2rem; text-align:center;border:1px solid #ff647c; color:#ff647c; font-size:0.12rem; border-radius:3px; margin:0 5px; font-weight:normal;}

.sTop h2{font-size:36px;font-weight:normal;}
#line .color1{color:#ff647c;}
#line .color2{color:#fff;}
.unit img{width:100%;}
#line .bor1{border-color:#fff;}
#line .bg1{background:#62b8f4;}
#line .bg2{background:#76c88d;}
#line .bg3{background:#65cdd8;}
#line .bg4{background:#01afff;}
#line .bg5{background:#00a0e9;}
#line .bg6{background:#6292f4;}
